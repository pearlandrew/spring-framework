{
  for (  String name : this.exchange.getRequestCookies().keySet()) {
    List<HttpCookie> list=map.get(name);
    if (list == null) {
      list=new ArrayList<>();
      map.put(name,list);
    }
    Cookie cookie=this.exchange.getRequestCookies().get(name);
    list.add(HttpCookie.clientCookie(name,cookie.getValue()));
  }
}
