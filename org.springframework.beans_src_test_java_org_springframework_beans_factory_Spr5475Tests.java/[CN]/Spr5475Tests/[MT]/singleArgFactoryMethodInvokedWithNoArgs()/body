{
  assertExceptionMessageForMisconfiguredFactoryMethod(rootBeanDefinition(Foo.class).setFactoryMethod("singleArgFactory").getBeanDefinition(),"Error creating bean with name 'foo': " + "Unsatisfied dependency expressed through constructor argument with index 0 of type [java.lang.String]: " + "Ambiguous factory method argument types - did you specify the correct bean references as factory method arguments?");
}
