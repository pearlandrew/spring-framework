{
  ConfigurableEnvironment env=new AbstractEnvironment(){
  }
;
  env.addPropertySource("ps1",new HashMap(){
{
      put("pName","ps1Value");
    }
  }
);
  assertThat(env.getProperty("pName"),equalTo("ps1Value"));
  env.addPropertySource("ps2",new HashMap(){
{
      put("pName","ps2Value");
    }
  }
);
  assertThat(env.getProperty("pName"),equalTo("ps2Value"));
  env.addPropertySource("ps3",new HashMap(){
{
      put("pName","ps3Value");
    }
  }
);
  assertThat(env.getProperty("pName"),equalTo("ps3Value"));
}
