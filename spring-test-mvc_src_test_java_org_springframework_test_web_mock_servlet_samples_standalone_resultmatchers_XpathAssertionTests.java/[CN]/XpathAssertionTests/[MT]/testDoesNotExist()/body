{
  String composer="/ns:people/composers/composer[%s]";
  String performer="/ns:people/performers/performer[%s]";
  this.mockMvc.perform(get("/music/people")).andExpect(xpath(composer,NS,0).doesNotExist()).andExpect(xpath(composer,NS,5).doesNotExist()).andExpect(xpath(performer,NS,0).doesNotExist()).andExpect(xpath(performer,NS,3).doesNotExist()).andExpect(xpath(composer,NS,0).node(nullValue()));
}
