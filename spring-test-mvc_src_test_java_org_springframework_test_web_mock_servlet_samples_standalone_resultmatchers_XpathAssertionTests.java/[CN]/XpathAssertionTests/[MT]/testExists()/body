{
  String composer="/ns:people/composers/composer[%s]";
  String performer="/ns:people/performers/performer[%s]";
  this.mockMvc.perform(get("/music/people")).andExpect(xpath(composer,NS,1).exists()).andExpect(xpath(composer,NS,2).exists()).andExpect(xpath(composer,NS,3).exists()).andExpect(xpath(composer,NS,4).exists()).andExpect(xpath(performer,NS,1).exists()).andExpect(xpath(performer,NS,2).exists()).andExpect(xpath(composer,NS,1).node(notNullValue()));
}
