{
  StringBuilder result=new StringBuilder();
  result.append("CacheDefinition[");
  result.append(name);
  result.append("] caches=");
  result.append(cacheNames);
  result.append(" | condition='");
  result.append(condition);
  result.append("' | key='");
  result.append(key);
  result.append("'");
  return result;
}
