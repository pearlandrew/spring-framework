{
  if (this.unavailableVariables.contains(name)) {
    throw new VariableNotAvailableException(name);
  }
  Object variable=super.lookupVariable(name);
  if (variable != null) {
    return variable;
  }
  if (!this.paramLoaded) {
    loadArgsAsVariables();
    this.paramLoaded=true;
    variable=super.lookupVariable(name);
  }
  return variable;
}
