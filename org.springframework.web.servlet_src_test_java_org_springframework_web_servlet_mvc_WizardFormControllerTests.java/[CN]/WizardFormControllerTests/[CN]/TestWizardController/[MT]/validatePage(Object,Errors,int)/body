{
  TestBean tb=(TestBean)command;
switch (page) {
case 0:
    if (tb.getName() == null) {
      errors.rejectValue("name","NAME_REQUIRED",null,"Name is required");
    }
  break;
case 1:
if (tb.getAge() == 0) {
  errors.rejectValue("age","AGE_REQUIRED",null,"Age is required");
}
break;
default :
throw new IllegalArgumentException("Invalid page number");
}
}
