{
  if (completed.get()) {
    return -1;
  }
  InputStream is=currentStream();
  if (is == null) {
    return -1;
  }
 else   if (b == null) {
    throw new NullPointerException();
  }
 else   if (off < 0 || len < 0 || len > b.length - off) {
    throw new IndexOutOfBoundsException();
  }
 else   if (len == 0) {
    return 0;
  }
  do {
    int n=is.read(b,off,len);
    if (n > 0) {
      return n;
    }
 else {
      is=currentStream();
    }
  }
 while (is != null);
  return -1;
}
