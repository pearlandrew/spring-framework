{
  String[] locations=new String[]{"foo","bar}"};
  MergedContextConfiguration mergedConfig1=new MergedContextConfiguration(getClass(),locations,EMPTY_CLASS_ARRAY,EMPTY_STRING_ARRAY,loader);
  MergedContextConfiguration mergedConfig2=new MergedContextConfiguration(getClass(),locations,EMPTY_CLASS_ARRAY,EMPTY_STRING_ARRAY,loader);
  assertEquals(mergedConfig1,mergedConfig2);
}
