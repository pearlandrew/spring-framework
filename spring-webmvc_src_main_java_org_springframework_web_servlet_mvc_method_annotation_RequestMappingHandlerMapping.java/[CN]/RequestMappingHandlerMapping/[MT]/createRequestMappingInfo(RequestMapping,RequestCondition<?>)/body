{
  String[] patterns=resolveEmbeddedValuesInPatterns(annotation.value());
  return new RequestMappingInfo(annotation.name(),new PatternsRequestCondition(patterns,getUrlPathHelper(),getPathMatcher(),this.useSuffixPatternMatch,this.useTrailingSlashMatch,this.fileExtensions),new RequestMethodsRequestCondition(annotation.method()),new ParamsRequestCondition(annotation.params()),new HeadersRequestCondition(annotation.headers()),new ConsumesRequestCondition(annotation.consumes(),annotation.headers()),new ProducesRequestCondition(annotation.produces(),annotation.headers(),this.contentNegotiationManager),customCondition);
}
