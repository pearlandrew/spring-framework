{
  this(declaringClass,resolveLocations(declaringClass,annAttrs.getStringArray("locations"),annAttrs.getStringArray("value")),annAttrs.getClassArray("classes"),annAttrs.getBoolean("inheritLocations"),(Class<? extends ApplicationContextInitializer<? extends ConfigurableApplicationContext>>[])annAttrs.getClassArray("initializers"),annAttrs.getBoolean("inheritInitializers"),annAttrs.getString("name"),(Class<? extends ContextLoader>)annAttrs.getClass("loader"));
}
