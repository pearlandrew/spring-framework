{
  A a=new A();
  C c=new C(3);
  B b=new B();
  Object[] items=new Object[]{a,c,b};
  comparator.sortArray(items,new OrderProvider(){
    @Override public Integer getOrder(    Object obj){
      if (obj == a) {
        return 4;
      }
      if (obj == b) {
        return 2;
      }
      return null;
    }
  }
);
  assertOrder(items,b,c,a);
}
