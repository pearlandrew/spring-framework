{
  A a=new A();
  C c=new C(3);
  B b=new B();
  List<?> items=Arrays.asList(a,c,b);
  comparator.sortList(items,new OrderProvider(){
    @Override public Integer getOrder(    Object obj){
      if (obj == a) {
        return 4;
      }
      if (obj == b) {
        return 2;
      }
      return null;
    }
  }
);
  assertOrder(items,b,c,a);
}
