{
  Assert.notNull(methodParameter,"MethodParameter must not be null");
  ResolvableType owner=forType(methodParameter.getContainingClass()).as(methodParameter.getDeclaringClass());
  return forType(SerializableTypeWrapper.forMethodParameter(methodParameter),owner.asVariableResolver()).getNested(methodParameter.getNestingLevel(),methodParameter.typeIndexesPerLevel);
}
