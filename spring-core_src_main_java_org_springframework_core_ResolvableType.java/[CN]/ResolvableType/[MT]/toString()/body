{
  if (isArray()) {
    return getComponentType() + "[]";
  }
  StringBuilder result=new StringBuilder();
  result.append(resolve() == null ? "?" : resolve().getName());
  if (hasGenerics()) {
    result.append('<');
    result.append(StringUtils.arrayToDelimitedString(getGenerics(),", "));
    result.append('>');
  }
  return result.toString();
}
