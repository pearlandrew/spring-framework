{
  if (getContentHandler() != null) {
    if (prefix == null) {
      prefix="";
    }
    if (!StringUtils.hasLength(namespace)) {
      return;
    }
    if (!namespace.equals(namespaces.get(prefix))) {
      getContentHandler().startPrefixMapping(prefix,namespace);
      namespaces.put(prefix,namespace);
    }
  }
}
