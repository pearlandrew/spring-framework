{
  runTestAndAssertCounters(testCaseType,EMPTY,expectedInvocationCounts.get(EMPTY),0,0);
  runTestAndAssertCounters(testCaseType,DISABLED_VIA_WRONG_NAME,expectedInvocationCounts.get(DISABLED_VIA_WRONG_NAME),0,0);
  runTestAndAssertCounters(testCaseType,DISABLED_VIA_WRONG_VALUE,expectedInvocationCounts.get(DISABLED_VIA_WRONG_VALUE),0,0);
  runTestAndAssertCounters(testCaseType,ENABLED_VIA_SINGLE_VALUE,expectedInvocationCounts.get(ENABLED_VIA_SINGLE_VALUE),0,0);
  runTestAndAssertCounters(testCaseType,ENABLED_VIA_MULTIPLE_VALUES,expectedInvocationCounts.get(ENABLED_VIA_MULTIPLE_VALUES),0,0);
  runTestAndAssertCounters(testCaseType,NOT_CONFIGURED,expectedInvocationCounts.get(NOT_CONFIGURED),0,0);
}
