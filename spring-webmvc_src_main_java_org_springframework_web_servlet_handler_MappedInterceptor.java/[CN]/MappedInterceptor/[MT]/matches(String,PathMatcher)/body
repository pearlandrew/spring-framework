{
  if (this.excludePatterns != null) {
    for (    String pattern : this.excludePatterns) {
      if (pathMatcher.match(pattern,lookupPath)) {
        return false;
      }
    }
  }
  if (this.includePatterns == null) {
    return true;
  }
 else {
    for (    String pattern : this.includePatterns) {
      if (pathMatcher.match(pattern,lookupPath)) {
        return true;
      }
    }
    return false;
  }
}
