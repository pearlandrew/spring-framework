{
  StringBuilder sb=new StringBuilder();
  if (getChildCount() > 0) {
    sb.append("(");
  }
  sb.append(getChild(0).toStringAST());
  for (int i=1; i < getChildCount(); i++) {
    sb.append(" ").append(getOperatorName()).append(" ");
    sb.append(getChild(i).toStringAST());
  }
  if (getChildCount() > 0) {
    sb.append(")");
  }
  return sb.toString();
}
