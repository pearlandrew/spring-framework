{
  addHeader(message,CLASSID_PROPERTY_NAME,(Class<?>)javaType.getRawClass());
  if (javaType.isContainerType()) {
    addHeader(message,CONTENT_CLASSID_PROPERTY_NAME,javaType.getContentType().getRawClass());
  }
  if (javaType.getKeyType() != null) {
    addHeader(message,KEY_CLASSID_PROPERTY_NAME,javaType.getKeyType().getRawClass());
  }
}
