{
  if (root != null && root.exists()) {
    if (root.isDirectory()) {
      File[] children=root.listFiles();
      if (children != null) {
        for (int i=0; i < children.length; i++) {
          deleteRecursively(children[i]);
        }
      }
    }
    return root.delete();
  }
  return false;
}
