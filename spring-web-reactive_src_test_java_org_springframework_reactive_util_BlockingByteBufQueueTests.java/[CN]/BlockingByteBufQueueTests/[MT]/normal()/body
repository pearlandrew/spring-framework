{
  byte[] abc=new byte[]{'a','b','c'};
  byte[] def=new byte[]{'d','e','f'};
  queue.putSignal(abc);
  queue.putSignal(def);
  queue.complete();
  assertTrue(queue.isHeadSignal());
  assertFalse(queue.isHeadError());
  assertSame(abc,queue.pollSignal());
  assertTrue(queue.isHeadSignal());
  assertFalse(queue.isHeadError());
  assertSame(def,queue.pollSignal());
  assertTrue(queue.isComplete());
}
