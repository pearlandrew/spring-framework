{
  ScriptTemplateConfigurer configurer=new ScriptTemplateConfigurer();
  configurer.setEngineName("nashorn");
  configurer.setScripts("org/springframework/web/servlet/view/script/handlebars/polyfill.js","/META-INF/resources/webjars/handlebars/3.0.0-1/handlebars.js","org/springframework/web/servlet/view/script/handlebars/render.js");
  configurer.setRenderFunction("render");
  return configurer;
}
