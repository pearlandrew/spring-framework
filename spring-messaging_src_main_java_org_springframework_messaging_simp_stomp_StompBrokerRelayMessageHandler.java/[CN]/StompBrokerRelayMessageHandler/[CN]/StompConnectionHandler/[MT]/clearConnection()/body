{
  this.isStompConnected=false;
  try {
    TcpConnection<byte[]> conn=this.tcpConnection;
    this.tcpConnection=null;
    if (conn != null) {
      conn.close();
    }
  }
  finally {
    if (this.isRemoteClientSession) {
      if (logger.isDebugEnabled()) {
        logger.debug("Removing session '" + sessionId + "' (total remaining="+ (StompBrokerRelayMessageHandler.this.connectionHandlers.size() - 1)+ ")");
      }
      StompBrokerRelayMessageHandler.this.connectionHandlers.remove(this.sessionId);
    }
  }
}
