{
  assertInTransaction(true);
  assertEquals("Adding jane",1,addPerson(super.simpleJdbcTemplate,JANE));
  assertEquals("Adding sue",1,addPerson(super.simpleJdbcTemplate,SUE));
  assertEquals("Verifying the number of rows in the person table in modifyTestDataWithinTransaction().",4,countRowsInPersonTable(super.simpleJdbcTemplate));
}
