{
  PersonDto source=new PersonDto();
  List<String> sports=new ArrayList<String>();
  sports.add("FOOTBALL");
  sports.add("BASKETBALL");
  source.setSports(sports);
  Person target=new Person();
  Mapper<PersonDto,Person> mapper=MapperFactory.mapperBuilder(PersonDto.class,Person.class).setAutoMappingEnabled(false).addMapping("sports","favoriteSports").getMapper();
  mapper.map(source,target);
  assertEquals(Sport.FOOTBALL,target.favoriteSports.get(0));
  assertEquals(Sport.BASKETBALL,target.favoriteSports.get(1));
}
