{
  PersonDto source=new PersonDto();
  NestedDto nested=new NestedDto();
  nested.foo="bar";
  source.setNested(nested);
  Person target=new Person();
  Mapper<PersonDto,Person> mapper=MapperFactory.mapperBuilder(PersonDto.class,Person.class).setAutoMappingEnabled(false).addMapping("nested").getMapper();
  mapper.map(source,target);
  assertEquals("bar",target.nested.foo);
}
