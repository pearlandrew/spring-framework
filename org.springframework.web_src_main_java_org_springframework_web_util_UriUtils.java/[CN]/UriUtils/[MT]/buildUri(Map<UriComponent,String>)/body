{
  Assert.notEmpty(uriComponents,"'uriComponents' must not be empty");
  return buildUri(uriComponents.get(UriComponent.SCHEME),uriComponents.get(UriComponent.AUTHORITY),uriComponents.get(UriComponent.USER_INFO),uriComponents.get(UriComponent.HOST),uriComponents.get(UriComponent.PORT),uriComponents.get(UriComponent.PATH),uriComponents.get(UriComponent.QUERY),uriComponents.get(UriComponent.FRAGMENT));
}
