{
  Assert.notEmpty(uriComponents,"'uriComponents' must not be empty");
  return buildUri(uriComponents.get(UriComponents.Type.SCHEME),uriComponents.get(UriComponents.Type.AUTHORITY),uriComponents.get(UriComponents.Type.USER_INFO),uriComponents.get(UriComponents.Type.HOST),uriComponents.get(UriComponents.Type.PORT),uriComponents.get(UriComponents.Type.PATH),uriComponents.get(UriComponents.Type.QUERY),uriComponents.get(UriComponents.Type.FRAGMENT));
}
