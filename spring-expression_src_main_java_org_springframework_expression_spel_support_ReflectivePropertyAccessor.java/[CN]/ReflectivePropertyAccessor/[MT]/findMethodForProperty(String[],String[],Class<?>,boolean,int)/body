{
  Method[] methods=getSortedClassMethods(clazz);
  for (  String methodSuffix : methodSuffixes) {
    for (    String prefix : prefixes) {
      for (      Method method : methods) {
        if (method.getName().equals(prefix + methodSuffix) && method.getParameterTypes().length == numberOfParams && (!mustBeStatic || Modifier.isStatic(method.getModifiers()))) {
          return method;
        }
      }
    }
  }
  return null;
}
