{
  SpelExpressionParser parser=new SpelExpressionParser();
  SpelExpression expr=parser.parse("2      +    3");
  Assert.assertEquals(5,expr.getValue());
  expr=parser.parse("2	+	3");
  Assert.assertEquals(5,expr.getValue());
  expr=parser.parse("2\n+	3");
  Assert.assertEquals(5,expr.getValue());
  expr=parser.parse("2\r\n+\t3");
  Assert.assertEquals(5,expr.getValue());
}
