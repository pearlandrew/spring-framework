{
  String[] paths=getConfigPaths();
  String[] locations=new String[paths.length];
  for (int i=0; i < paths.length; i++) {
    String path=paths[i];
    if (path.startsWith("/")) {
      locations[i]=ResourceUtils.CLASSPATH_URL_PREFIX + path;
    }
 else {
      locations[i]=ResourceUtils.CLASSPATH_URL_PREFIX + StringUtils.cleanPath(ClassUtils.classPackageAsResourcePath(getClass()) + "/" + path);
    }
  }
  return locations;
}
