{
  final Long ID_1=13L;
  Person found1=new Person();
  Person.findPerson(ID_1);
  expectReturn(found1);
  playback();
  assertEquals(found1,Person.findPerson(ID_1));
  verify();
  reset();
  final long ID_2=ID_1 + 1;
  Person found2=new Person();
  Person.findPerson(ID_2);
  expectReturn(found2);
  playback();
  assertEquals(found2,Person.findPerson(ID_2));
}
