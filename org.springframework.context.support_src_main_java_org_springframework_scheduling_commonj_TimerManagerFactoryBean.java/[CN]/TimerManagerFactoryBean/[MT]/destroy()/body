{
  for (Iterator it=this.timers.iterator(); it.hasNext(); ) {
    Timer timer=(Timer)it.next();
    try {
      timer.cancel();
    }
 catch (    Throwable ex) {
      logger.warn("Could not cancel CommonJ Timer",ex);
    }
  }
  this.timers.clear();
  if (!this.shared) {
    this.timerManager.stop();
  }
}
