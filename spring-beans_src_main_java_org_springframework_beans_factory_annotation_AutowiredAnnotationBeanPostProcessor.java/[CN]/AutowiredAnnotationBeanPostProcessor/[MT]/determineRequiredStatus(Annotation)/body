{
  try {
    Method method=ReflectionUtils.findMethod(annotation.annotationType(),this.requiredParameterName);
    if (method == null) {
      return true;
    }
    return (this.requiredParameterValue == (Boolean)ReflectionUtils.invokeMethod(method,annotation));
  }
 catch (  Exception ex) {
    return true;
  }
}
