{
  if (Boolean.TRUE.equals(target)) {
    if (trueString != null) {
      return trueString;
    }
 else {
      return VALUE_TRUE;
    }
  }
 else   if (Boolean.FALSE.equals(target)) {
    if (falseString != null) {
      return falseString;
    }
 else {
      return VALUE_FALSE;
    }
  }
 else {
    throw new IllegalArgumentException("Invalid boolean value [" + target + "]");
  }
}
