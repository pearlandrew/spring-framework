{
  this.mockServer.expect(requestTo("/composers")).andExpect(content().contentType("application/xml")).andExpect(xpath("/ns:people/composers/composer",NS).nodeCount(4)).andExpect(xpath("/ns:people/performers/performer",NS).nodeCount(2)).andExpect(xpath("/ns:people/composers/composer",NS).nodeCount(lessThan(5))).andExpect(xpath("/ns:people/performers/performer",NS).nodeCount(greaterThan(0))).andRespond(withSuccess());
  this.restTemplate.put(new URI("/composers"),this.people);
  this.mockServer.verify();
}
