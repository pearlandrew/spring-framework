{
  Flux<Pojo> source=Flux.just(new Pojo("foofoo","barbar"),new Pojo("foofoofoo","barbarbar"));
  Flux<DataBuffer> output=this.encoder.encode(source,this.dataBufferFactory,null,null);
  TestSubscriber.subscribe(output).assertComplete().assertNoError().assertValuesWith(stringConsumer("["),stringConsumer("{\"foo\":\"foofoo\",\"bar\":\"barbar\"}"),stringConsumer(","),stringConsumer("{\"foo\":\"foofoofoo\",\"bar\":\"barbarbar\"}"),stringConsumer("]"));
}
