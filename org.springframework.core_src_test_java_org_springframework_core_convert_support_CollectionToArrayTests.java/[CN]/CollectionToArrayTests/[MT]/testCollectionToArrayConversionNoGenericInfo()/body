{
  DefaultTypeConverter service=new DefaultTypeConverter();
  CollectionToArray c=new CollectionToArray(ConversionContext.valueOf(Collection.class),ConversionContext.valueOf(Integer[].class),service);
  bindTarget.add("1");
  bindTarget.add("2");
  bindTarget.add("3");
  Integer[] result=(Integer[])c.execute(bindTarget);
  assertEquals(new Integer(1),result[0]);
  assertEquals(new Integer(2),result[1]);
  assertEquals(new Integer(3),result[2]);
}
