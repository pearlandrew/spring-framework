{
  if (getContentHandler() != null) {
    QName qName=startElement.getName();
    if (hasNamespacesFeature()) {
      for (Iterator i=startElement.getNamespaces(); i.hasNext(); ) {
        Namespace namespace=(Namespace)i.next();
        getContentHandler().startPrefixMapping(namespace.getPrefix(),namespace.getNamespaceURI());
      }
      for (Iterator i=startElement.getAttributes(); i.hasNext(); ) {
        Attribute attribute=(Attribute)i.next();
        String prefix=attribute.getName().getPrefix();
        if (prefix == null) {
          prefix="";
        }
        String namespace=attribute.getName().getNamespaceURI();
        if (namespace == null) {
          continue;
        }
        getContentHandler().startPrefixMapping(prefix,namespace);
      }
      getContentHandler().startElement(qName.getNamespaceURI(),qName.getLocalPart(),toQualifiedName(qName),getAttributes(startElement));
    }
 else {
      getContentHandler().startElement("","",toQualifiedName(qName),getAttributes(startElement));
    }
  }
}
