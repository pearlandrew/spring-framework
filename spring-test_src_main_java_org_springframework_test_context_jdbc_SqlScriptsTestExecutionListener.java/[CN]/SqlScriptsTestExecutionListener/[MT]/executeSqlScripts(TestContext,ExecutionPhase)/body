{
  boolean classLevel=false;
  Set<Sql> sqlAnnotations=AnnotationUtils.getRepeatableAnnotations(testContext.getTestMethod(),Sql.class,SqlGroup.class);
  if (sqlAnnotations.isEmpty()) {
    sqlAnnotations=AnnotationUtils.getRepeatableAnnotations(testContext.getTestClass(),Sql.class,SqlGroup.class);
    if (!sqlAnnotations.isEmpty()) {
      classLevel=true;
    }
  }
  for (  Sql sql : sqlAnnotations) {
    executeSqlScripts(sql,executionPhase,testContext,classLevel);
  }
}
