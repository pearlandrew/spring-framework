{
  ObjectMapper mapper;
  if (this.createXmlMapper) {
    mapper=(this.defaultUseWrapper != null ? new XmlObjectMapperInitializer().create(this.defaultUseWrapper) : new XmlObjectMapperInitializer().create());
  }
 else {
    mapper=new ObjectMapper();
  }
  configure(mapper);
  return (T)mapper;
}
