{
  sfControl.reset();
  sessionControl.reset();
  sf.openSession();
  sfControl.setReturnValue(session);
  session.getSessionFactory();
  sessionControl.setReturnValue(sf,1);
  session.close();
  sessionControl.setReturnValue(null,1);
  sfControl.replay();
  sessionControl.replay();
  return new HibernateTemplate(sf);
}
