{
  Flux<DataBuffer> source=Flux.just(stringBuffer("{\"foo\": \"foofoo\", \"bar\": \"barbar\"}"));
  Flux<Object> output=this.decoder.decode(source,ResolvableType.forClass(Pojo.class),null);
  TestSubscriber<Object> testSubscriber=new TestSubscriber<>();
  testSubscriber.bindTo(output).assertValues(new Pojo("foofoo","barbar"));
}
