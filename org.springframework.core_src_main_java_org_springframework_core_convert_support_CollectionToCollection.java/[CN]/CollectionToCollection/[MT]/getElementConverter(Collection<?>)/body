{
  ConversionExecutor elementConverter=getElementConverter();
  if (elementConverter == NoOpConversionExecutor.INSTANCE && getTargetElementType() != null) {
    Iterator<?> it=source.iterator();
    while (it.hasNext()) {
      Object value=it.next();
      if (value != null) {
        elementConverter=getConversionService().getConversionExecutor(value.getClass(),BindingPoint.valueOf(getTargetElementType()));
        break;
      }
    }
  }
  return elementConverter;
}
