{
  Collection<T> anns=new ArrayList<T>(2);
  T ann=ae.getAnnotation(annotationType);
  if (ann != null) {
    anns.add(ann);
  }
  for (  Annotation metaAnn : ae.getAnnotations()) {
    ann=metaAnn.annotationType().getAnnotation(annotationType);
    if (ann != null) {
      anns.add(ann);
    }
  }
  return anns;
}
