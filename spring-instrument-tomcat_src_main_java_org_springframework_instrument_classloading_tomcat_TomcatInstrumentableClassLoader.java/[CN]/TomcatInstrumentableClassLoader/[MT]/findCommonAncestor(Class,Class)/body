{
  Class ancestor=one;
  while (ancestor != Object.class || ancestor != null) {
    if (ancestor.isAssignableFrom(two)) {
      return ancestor;
    }
    ancestor=ancestor.getSuperclass();
  }
  ancestor=two;
  while (ancestor != Object.class || ancestor != null) {
    if (ancestor.isAssignableFrom(one)) {
      return ancestor;
    }
    ancestor=ancestor.getSuperclass();
  }
  return null;
}
