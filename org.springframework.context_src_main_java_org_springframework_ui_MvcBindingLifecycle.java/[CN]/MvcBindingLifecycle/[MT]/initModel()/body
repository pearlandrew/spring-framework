{
  try {
    model=modelType.newInstance();
  }
 catch (  InstantiationException e) {
    throw new IllegalStateException("Model of type [" + modelType.getName() + "] does not have a default constructor");
  }
catch (  IllegalAccessException e) {
    throw new IllegalStateException("Model of type [" + modelType.getName() + "] does not have a public default constructor");
  }
}
