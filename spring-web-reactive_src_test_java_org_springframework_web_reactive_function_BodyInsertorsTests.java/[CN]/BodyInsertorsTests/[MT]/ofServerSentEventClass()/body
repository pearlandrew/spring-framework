{
  Flux<String> body=Flux.just("foo");
  BodyInsertor<Flux<String>> insertor=BodyInsertors.fromServerSentEvents(body,String.class);
  assertEquals(body,insertor.t());
  MockServerHttpResponse response=new MockServerHttpResponse();
  Mono<Void> result=insertor.insert(response,Configuration.builder().build());
  TestSubscriber.subscribe(result).assertComplete();
}
