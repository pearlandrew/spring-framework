{
  try {
    return (Map)System.getProperties();
  }
 catch (  AccessControlException ex) {
    return (Map)new ReadOnlySystemAttributesMap(){
      @Override protected String getSystemAttribute(      String attributeName){
        try {
          return System.getProperty(attributeName);
        }
 catch (        AccessControlException ex) {
          if (logger.isInfoEnabled()) {
            logger.info(format("Caught AccessControlException when accessing system " + "property [%s]; its value will be returned [null]. Reason: %s",attributeName,ex.getMessage()));
          }
          return null;
        }
      }
    }
;
  }
}
