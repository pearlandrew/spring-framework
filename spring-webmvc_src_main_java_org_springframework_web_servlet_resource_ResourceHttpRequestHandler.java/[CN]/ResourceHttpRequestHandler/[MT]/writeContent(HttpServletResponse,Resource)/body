{
  try {
    InputStream in=resource.getInputStream();
    try {
      StreamUtils.copy(in,response.getOutputStream());
    }
 catch (    NullPointerException ex) {
    }
 finally {
      try {
        in.close();
      }
 catch (      Throwable ex) {
      }
    }
  }
 catch (  FileNotFoundException ex) {
  }
}
