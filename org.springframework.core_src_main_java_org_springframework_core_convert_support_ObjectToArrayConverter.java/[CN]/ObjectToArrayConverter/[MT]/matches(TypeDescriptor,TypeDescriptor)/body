{
  boolean canConvert=conversionService.canConvert(sourceType,targetType.getElementTypeDescriptor());
  if (canConvert) {
    return true;
  }
 else {
    if (sourceType.getType().isAssignableFrom(targetType.getElementTypeDescriptor().getType())) {
      return true;
    }
 else {
      return false;
    }
  }
}
