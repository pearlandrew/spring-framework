{
  EvaluationContext eContext=state.getEvaluationContext();
  List<ConstructorResolver> cResolvers=eContext.getConstructorResolvers();
  if (cResolvers != null) {
    for (    ConstructorResolver ctorResolver : cResolvers) {
      try {
        ConstructorExecutor cEx=ctorResolver.resolve(state.getEvaluationContext(),typename,argumentTypes);
        if (cEx != null) {
          return cEx;
        }
      }
 catch (      AccessException ex) {
        throw new SpelException(ex,SpelMessages.PROBLEM_LOCATING_CONSTRUCTOR,typename,FormatHelper.formatMethodForMessage("",argumentTypes));
      }
    }
  }
  throw new SpelException(SpelMessages.CONSTRUCTOR_NOT_FOUND,typename,FormatHelper.formatMethodForMessage("",argumentTypes));
}
