{
  String name="something";
  final Context context=createMock(Context.class);
  context.unbind(name);
  context.close();
  replay(context);
  JndiTemplate jt=new JndiTemplate(){
    protected Context createInitialContext(){
      return context;
    }
  }
;
  jt.unbind(name);
  verify(context);
}
