{
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(ByteBuffer.class),MimeTypeUtils.TEXT_PLAIN,Collections.emptyMap()));
  assertFalse(this.encoder.canEncode(ResolvableType.forClass(Integer.class),MimeTypeUtils.TEXT_PLAIN,Collections.emptyMap()));
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(ByteBuffer.class),MimeTypeUtils.APPLICATION_JSON,Collections.emptyMap()));
}
