{
  if (headers == null) {
    headers=new HttpHeaders();
    int i=1;
    while (true) {
      String name=connection.getHeaderFieldKey(i);
      if (!StringUtils.hasLength(name)) {
        break;
      }
      headers.add(name,connection.getHeaderField(i));
      i++;
    }
  }
  return headers;
}
