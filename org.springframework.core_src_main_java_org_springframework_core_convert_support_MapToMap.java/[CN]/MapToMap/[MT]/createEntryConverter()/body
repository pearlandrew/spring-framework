{
  if (sourceType.isMapEntryTypeKnown() && targetType.isMapEntryTypeKnown()) {
    ConversionExecutor keyConverter=conversionService.getConversionExecutor(sourceType.getMapKeyType(),ConversionPoint.valueOf(targetType.getMapKeyType()));
    ConversionExecutor valueConverter=conversionService.getConversionExecutor(sourceType.getMapValueType(),ConversionPoint.valueOf(targetType.getMapValueType()));
    return new EntryConverter(keyConverter,valueConverter);
  }
 else {
    return EntryConverter.NO_OP_INSTANCE;
  }
}
