{
  if (sourceType.isMapEntryTypeKnown() && targetType.isMapEntryTypeKnown()) {
    ConversionExecutor keyConverter=conversionService.getConversionExecutor(sourceType.getMapKeyType(),TypeDescriptor.valueOf(targetType.getMapKeyType()));
    ConversionExecutor valueConverter=conversionService.getConversionExecutor(sourceType.getMapValueType(),TypeDescriptor.valueOf(targetType.getMapValueType()));
    return new MapEntryConverter(keyConverter,valueConverter);
  }
 else {
    return MapEntryConverter.NO_OP_INSTANCE;
  }
}
