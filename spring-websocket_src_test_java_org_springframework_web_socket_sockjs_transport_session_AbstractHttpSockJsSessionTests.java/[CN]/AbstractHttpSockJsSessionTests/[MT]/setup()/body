{
  super.setUp();
  this.frameFormat=new DefaultFrameFormat("%s");
  this.servletResponse=new MockHttpServletResponse();
  this.response=new ServletServerHttpResponse(this.servletResponse);
  this.servletRequest=new MockHttpServletRequest();
