{
  for (  Cookie cookie : this.request.getCookies()) {
    String name=cookie.getName();
    Set<HttpCookie> set=map.get(name);
    if (set == null) {
      set=new LinkedHashSet<>();
      map.put(name,set);
    }
    set.add(new HttpCookie(name,cookie.getValue()).setDomain(cookie.getDomain()).setPath(cookie.getPath()).setMaxAge(cookie.getMaxAge() == -1 ? Long.MIN_VALUE : cookie.getMaxAge()).setHttpOnly(cookie.isHttpOnly()).setSecure(cookie.getSecure()));
  }
}
