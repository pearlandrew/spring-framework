{
synchronized (pw) {
    super.printStackTrace(pw);
    if (this.relatedCauses != null) {
      for (Iterator it=this.relatedCauses.iterator(); it.hasNext(); ) {
        Throwable relatedCause=(Throwable)it.next();
        pw.println("Related cause:");
        relatedCause.printStackTrace(pw);
      }
    }
  }
}
