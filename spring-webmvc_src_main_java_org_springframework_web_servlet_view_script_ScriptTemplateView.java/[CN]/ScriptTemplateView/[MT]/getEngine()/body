{
  if (Boolean.FALSE.equals(this.sharedEngine)) {
    ScriptEngine engine=this.engineHolder.get();
    if (engine == null) {
      engine=createEngineFromName();
      this.engineHolder.set(engine);
    }
    return engine;
  }
 else   if (this.engine == null) {
    setEngine(createEngineFromName());
  }
  return this.engine;
}
