{
  Class<?> paramType=param.getNestedParameterType();
  if (Collection.class.equals(paramType) || List.class.isAssignableFrom(paramType)) {
    Class<?> valueType=GenericCollectionTypeResolver.getCollectionParameterType(param);
    if (valueType != null) {
      return valueType;
    }
  }
  return null;
}
