{
  NettyDataBufferFactory dataBufferFactory=new NettyDataBufferFactory(channel.delegate().alloc());
  ReactorServerHttpRequest adaptedRequest=new ReactorServerHttpRequest(channel,dataBufferFactory);
  ReactorServerHttpResponse adaptedResponse=new ReactorServerHttpResponse(channel,dataBufferFactory);
  return this.httpHandler.handle(adaptedRequest,adaptedResponse);
}
