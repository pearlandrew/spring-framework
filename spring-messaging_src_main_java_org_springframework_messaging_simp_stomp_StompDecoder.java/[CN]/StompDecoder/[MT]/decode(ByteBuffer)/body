{
  skipLeadingEol(buffer);
  Message<byte[]> decodedMessage;
  String command=readCommand(buffer);
  if (command.length() > 0) {
    MultiValueMap<String,String> headers=readHeaders(buffer);
    byte[] payload=readPayload(buffer,headers);
    decodedMessage=MessageBuilder.withPayloadAndHeaders(payload,StompHeaderAccessor.create(StompCommand.valueOf(command),headers)).build();
  }
 else {
    decodedMessage=MessageBuilder.withPayloadAndHeaders(HEARTBEAT_PAYLOAD,StompHeaderAccessor.create(SimpMessageType.HEARTBEAT)).build();
  }
  if (logger.isTraceEnabled()) {
    logger.trace("Decoded " + decodedMessage);
  }
  return decodedMessage;
}
