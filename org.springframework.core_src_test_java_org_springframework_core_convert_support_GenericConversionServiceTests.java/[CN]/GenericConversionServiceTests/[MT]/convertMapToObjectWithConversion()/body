{
  Map<Long,Long> foo=new LinkedHashMap<Long,Long>();
  foo.put(1L,1L);
  foo.put(2L,2L);
  conversionService.addConverterFactory(new NumberToNumberConverterFactory());
  Integer result=(Integer)conversionService.convert(foo,new TypeDescriptor(getClass().getField("genericMap2")),TypeDescriptor.valueOf(Integer.class));
  assertEquals(new Integer(1),result);
}
