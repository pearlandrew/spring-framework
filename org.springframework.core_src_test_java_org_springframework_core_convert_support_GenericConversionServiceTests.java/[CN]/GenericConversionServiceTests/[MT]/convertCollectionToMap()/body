{
  conversionService.addGenericConverter(Collection.class,Map.class,new CollectionToMapConverter(conversionService));
  List<String> list=new ArrayList<String>();
  list.add("foo=bar");
  list.add("bar=baz");
  list.add("baz=boop");
  Map result=conversionService.convert(list,Map.class);
  assertEquals("bar",result.get("foo"));
  assertEquals("baz",result.get("bar"));
  assertEquals("boop",result.get("baz"));
}
