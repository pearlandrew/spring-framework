{
  if (targetType.isMapEntryTypeKnown()) {
    ConversionExecutor keyConverter=conversionService.getConversionExecutor(String.class,TypeDescriptor.valueOf(targetType.getMapKeyType()));
    ConversionExecutor valueConverter=conversionService.getConversionExecutor(String.class,TypeDescriptor.valueOf(targetType.getMapValueType()));
    return new EntryConverter(keyConverter,valueConverter);
  }
 else {
    return EntryConverter.NO_OP_INSTANCE;
  }
}
