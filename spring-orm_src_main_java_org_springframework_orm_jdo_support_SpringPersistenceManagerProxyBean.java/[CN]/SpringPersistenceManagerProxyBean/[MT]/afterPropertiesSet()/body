{
  if (getPersistenceManagerFactory() == null) {
    throw new IllegalArgumentException("Property 'persistenceManagerFactory' is required");
  }
  if (this.jdoDialect == null) {
    this.jdoDialect=new DefaultJdoDialect(getPersistenceManagerFactory().getConnectionFactory());
  }
  this.proxy=(PersistenceManager)Proxy.newProxyInstance(getPersistenceManagerFactory().getClass().getClassLoader(),new Class<?>[]{getPersistenceManagerInterface()},new PersistenceManagerInvocationHandler());
}
