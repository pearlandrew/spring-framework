{
  for (  String name : this.channel.cookies().keySet()) {
    List<HttpCookie> list=cookies.get(name);
    if (list == null) {
      list=new ArrayList<>();
      cookies.put(name,list);
    }
    for (    Cookie cookie : this.channel.cookies().get(name)) {
      list.add(HttpCookie.clientCookie(name,cookie.value()));
    }
  }
}
