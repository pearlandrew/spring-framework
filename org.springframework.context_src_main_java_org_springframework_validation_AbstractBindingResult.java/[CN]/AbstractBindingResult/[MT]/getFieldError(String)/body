{
  String fixedField=fixedField(field);
  for (Iterator it=this.errors.iterator(); it.hasNext(); ) {
    Object error=it.next();
    if (error instanceof FieldError) {
      FieldError fe=(FieldError)error;
      if (isMatchingFieldError(fixedField,fe)) {
        return fe;
      }
    }
  }
  return null;
}
