{
  Expression expr=null;
  expr=new SpelExpressionParser().parseRaw("?:'default'");
  Assert.assertEquals("default",expr.getValue());
  expr=new SpelExpressionParser().parseRaw("\"\"?:'default'");
  Assert.assertEquals("default",expr.getValue());
  expr=new SpelExpressionParser().parseRaw("''?:'default'");
  Assert.assertEquals("default",expr.getValue());
}
