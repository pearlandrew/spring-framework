{
  try {
    this.asyncRequestControl.start(-1);
    this.requestInitialized=true;
    scheduleHeartbeat();
    tryFlushCache();
  }
 catch (  Throwable ex) {
    tryCloseWithSockJsTransportError(ex,CloseStatus.SERVER_ERROR);
    throw new SockJsTransportFailureException("Failed to flush messages",getId(),ex);
  }
}
