{
  if (contentId.startsWith(CID)) {
    contentId=contentId.substring(CID.length());
    try {
      contentId=URLDecoder.decode(contentId,"UTF-8");
    }
 catch (    UnsupportedEncodingException e) {
    }
    contentId='<' + contentId + '>';
  }
  return mimeContainer.getAttachment(contentId);
}
