{
  if (logger.isInfoEnabled()) {
    logger.info("Creating JAXBContext with classes to be bound [" + StringUtils.arrayToCommaDelimitedString(classesToBeBound) + "]");
  }
  if (jaxbContextProperties != null) {
    return JAXBContext.newInstance(classesToBeBound,jaxbContextProperties);
  }
 else {
    return JAXBContext.newInstance(classesToBeBound);
  }
}
