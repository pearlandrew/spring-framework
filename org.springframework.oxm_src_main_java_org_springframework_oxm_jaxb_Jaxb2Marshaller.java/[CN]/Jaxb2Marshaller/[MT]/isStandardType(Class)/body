{
  return (String.class.equals(clazz) || BigInteger.class.equals(clazz) || BigDecimal.class.equals(clazz)|| Calendar.class.isAssignableFrom(clazz)|| Date.class.isAssignableFrom(clazz)|| QName.class.equals(clazz)|| URI.class.equals(clazz)|| XMLGregorianCalendar.class.isAssignableFrom(clazz)|| Duration.class.isAssignableFrom(clazz)|| Object.class.equals(clazz)|| Image.class.isAssignableFrom(clazz)|| DataHandler.class.equals(clazz)|| Source.class.isAssignableFrom(clazz)|| UUID.class.equals(clazz));
