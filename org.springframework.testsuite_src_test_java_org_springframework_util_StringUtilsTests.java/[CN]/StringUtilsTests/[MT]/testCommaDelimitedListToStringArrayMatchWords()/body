{
  String[] sa=new String[]{"foo","bar","big"};
  doTestCommaDelimitedListToStringArrayLegalMatch(sa);
  testStringArrayReverseTransformationMatches(sa);
  sa=new String[]{"a","b","c"};
  doTestCommaDelimitedListToStringArrayLegalMatch(sa);
  testStringArrayReverseTransformationMatches(sa);
  sa=new String[]{"AA","AA","AA","AA","AA"};
  doTestCommaDelimitedListToStringArrayLegalMatch(sa);
  testStringArrayReverseTransformationMatches(sa);
}
