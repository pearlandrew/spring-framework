{
  if (exType == null) {
    return false;
  }
  if (exType.isInstance(this)) {
    return true;
  }
  for (int i=0; i < this.propertyAccessExceptions.length; i++) {
    PropertyAccessException pae=this.propertyAccessExceptions[i];
    if (pae.contains(exType)) {
      return true;
    }
  }
  return false;
}
