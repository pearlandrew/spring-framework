{
  AnnotationConfigApplicationContext ctx=new AnnotationConfigApplicationContext();
  ctx.register(FeatureConfig.class);
  try {
    ctx.refresh();
    fail("expected exception");
  }
 catch (  FeatureMethodExecutionException ex) {
    assertThat(ex.getCause().getMessage(),equalTo("Return type for @Feature method FeatureConfig.f() must be " + "assignable to FeatureSpecification"));
  }
}
