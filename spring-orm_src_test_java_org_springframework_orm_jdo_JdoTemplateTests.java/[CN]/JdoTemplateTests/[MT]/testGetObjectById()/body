{
  pmf.getPersistenceManager();
  pmfControl.setReturnValue(pm);
  pm.getObjectById("0",true);
  pmControl.setReturnValue("A");
  pm.close();
  pmControl.setVoidCallable(1);
  pmfControl.replay();
  pmControl.replay();
  JdoTemplate jt=new JdoTemplate(pmf);
  assertEquals("A",jt.getObjectById("0"));
}
