{
  pmfControl.reset();
  pmControl.reset();
  pmf.getConnectionFactory();
  pmfControl.setReturnValue(null,1);
  pmf.getPersistenceManager();
  pmfControl.setReturnValue(pm,1);
  pm.close();
  pmControl.setVoidCallable(1);
  pmfControl.replay();
  pmControl.replay();
  return new JdoTemplate(pmf);
}
