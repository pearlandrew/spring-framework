{
  Collection coll=new HashSet();
  pmf.getPersistenceManager();
  pmfControl.setReturnValue(pm);
  pm.refreshAll(coll);
  pmControl.setVoidCallable(1);
  pm.close();
  pmControl.setVoidCallable(1);
  pmfControl.replay();
  pmControl.replay();
  JdoTemplate jt=new JdoTemplate(pmf);
  jt.refreshAll(coll);
}
