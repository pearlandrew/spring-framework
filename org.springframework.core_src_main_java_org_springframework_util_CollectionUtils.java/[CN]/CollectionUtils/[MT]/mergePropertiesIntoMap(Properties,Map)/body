{
  if (map == null) {
    throw new IllegalArgumentException("Map must not be null");
  }
  if (props != null) {
    for (Enumeration en=props.propertyNames(); en.hasMoreElements(); ) {
      String key=(String)en.nextElement();
      Object value=props.getProperty(key);
      if (value == null) {
        value=props.get(key);
      }
      map.put(key,value);
    }
  }
}
