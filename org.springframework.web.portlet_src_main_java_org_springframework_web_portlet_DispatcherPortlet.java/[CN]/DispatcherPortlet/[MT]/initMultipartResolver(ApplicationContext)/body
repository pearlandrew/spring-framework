{
  try {
    this.multipartResolver=(PortletMultipartResolver)context.getBean(MULTIPART_RESOLVER_BEAN_NAME,PortletMultipartResolver.class);
    if (logger.isDebugEnabled()) {
      logger.debug("Using MultipartResolver [" + this.multipartResolver + "]");
    }
  }
 catch (  NoSuchBeanDefinitionException ex) {
    this.multipartResolver=null;
    if (logger.isDebugEnabled()) {
      logger.debug("Unable to locate PortletMultipartResolver with name '" + MULTIPART_RESOLVER_BEAN_NAME + "': no multipart request handling provided");
    }
  }
}
