{
  if (source == null) {
    return null;
  }
  if (sourceType != null && !sourceType.isInstance(source)) {
    throw new ConversionExecutionException(source,getSourceType(),getTargetType(),"Source object " + source + " to convert is expected to be an instance of ["+ getSourceType().getName()+ "]");
  }
  try {
    return converter.convert(source);
  }
 catch (  Exception e) {
    throw new ConversionExecutionException(source,getSourceType(),getTargetType(),e);
  }
}
