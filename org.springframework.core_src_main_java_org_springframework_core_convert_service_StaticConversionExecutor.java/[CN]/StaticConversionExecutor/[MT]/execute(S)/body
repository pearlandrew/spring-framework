{
  if (source == null) {
    return null;
  }
  if (!sourceClass.isInstance(source)) {
    throw new ConversionExecutionException(source,getSourceClass(),getTargetClass(),"Source object " + source + " to convert is expected to be an instance of ["+ getSourceClass().getName()+ "]");
  }
  try {
    return converter.convert(source);
  }
 catch (  Exception e) {
    throw new ConversionExecutionException(source,getSourceClass(),getTargetClass(),e);
  }
}
