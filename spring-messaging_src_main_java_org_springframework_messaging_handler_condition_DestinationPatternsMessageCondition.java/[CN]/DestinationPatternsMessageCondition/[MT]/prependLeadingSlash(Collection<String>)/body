{
  if (patterns == null) {
    return Collections.emptySet();
  }
  Set<String> result=new LinkedHashSet<String>(patterns.size());
  for (  String pattern : patterns) {
    if (StringUtils.hasLength(pattern) && !pattern.startsWith("/")) {
      pattern="/" + pattern;
    }
    result.add(pattern);
  }
  return result;
}
