{
  if (sharedConnectionEnabled()) {
synchronized (sharedConnectionMonitor) {
      JmsUtils.closeMessageConsumer(this.consumer);
      JmsUtils.closeSession(this.session);
    }
  }
 else {
    JmsUtils.closeMessageConsumer(this.consumer);
    JmsUtils.closeSession(this.session);
  }
  this.consumer=null;
  this.session=null;
}
