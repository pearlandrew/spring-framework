{
  this.tilesPropertyMap.put(TilesContainerFactory.CONTAINER_FACTORY_INIT_PARAM,TilesContainerFactory.class.getName());
  this.tilesPropertyMap.put(TilesContainerFactory.CONTEXT_FACTORY_INIT_PARAM,ChainedTilesContextFactory.class.getName());
  this.tilesPropertyMap.put(TilesContainerFactory.DEFINITIONS_FACTORY_INIT_PARAM,UrlDefinitionsFactory.class.getName());
  this.tilesPropertyMap.put(TilesContainerFactory.PREPARER_FACTORY_INIT_PARAM,BasicPreparerFactory.class.getName());
  this.tilesPropertyMap.put(ChainedTilesContextFactory.FACTORY_CLASS_NAMES,ServletTilesContextFactory.class.getName() + "," + JspTilesContextFactory.class.getName());
  this.tilesPropertyMap.put(UrlDefinitionsFactory.LOCALE_RESOLVER_IMPL_PROPERTY,SpringLocaleResolver.class.getName());
}
