{
  if (returnType.getMethodAnnotation(SendTo.class) != null || AnnotationUtils.getAnnotation(returnType.getDeclaringClass(),SendTo.class) != null || returnType.getMethodAnnotation(SendToUser.class) != null || AnnotationUtils.getAnnotation(returnType.getDeclaringClass(),SendToUser.class) != null) {
    return true;
  }
  return (!this.annotationRequired);
}
