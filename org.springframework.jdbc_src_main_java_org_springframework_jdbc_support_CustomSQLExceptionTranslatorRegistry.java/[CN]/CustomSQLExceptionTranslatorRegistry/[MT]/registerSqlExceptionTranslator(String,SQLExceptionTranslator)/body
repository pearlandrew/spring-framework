{
  SQLExceptionTranslator replaced=sqlExceptionTranslatorRegistry.put(dbName,sqlExceptionTranslator);
  if (replaced != null) {
    logger.warn("Replacing custom translator '" + replaced + "' for database "+ dbName+ " with '"+ sqlExceptionTranslator+ "'");
  }
 else {
    logger.info("Adding custom translator '" + sqlExceptionTranslator.getClass().getSimpleName() + "' for database "+ dbName);
  }
}
