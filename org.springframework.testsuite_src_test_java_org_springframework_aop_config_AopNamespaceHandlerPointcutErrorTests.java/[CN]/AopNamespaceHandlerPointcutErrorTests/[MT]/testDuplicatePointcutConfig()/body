{
  try {
    new XmlBeanFactory(new ClassPathResource("org/springframework/aop/config/aopNamespaceHandlerPointcutDuplicationTests.xml"));
    fail("parsing should have caused a BeanDefinitionStoreException");
  }
 catch (  BeanDefinitionStoreException ex) {
    assertTrue(ex.contains(BeanDefinitionParsingException.class));
  }
}
