{
  String value=source.trim();
  if (value.length() == 0) {
    return null;
  }
 else   if (trueValues.contains(value)) {
    return Boolean.TRUE;
  }
 else   if (falseValues.contains(value)) {
    return Boolean.FALSE;
  }
 else {
    throw new IllegalArgumentException("Invalid boolean value '" + source + "'");
  }
}
