{
  if (obj instanceof Ordered) {
    return ((Ordered)obj).getOrder();
  }
  if (obj != null) {
    Class<?> clazz=(obj instanceof Class ? (Class)obj : obj.getClass());
    Order order=AnnotationUtils.findAnnotation(clazz,Order.class);
    if (order != null) {
      return order.value();
    }
  }
  return Ordered.LOWEST_PRECEDENCE;
}
