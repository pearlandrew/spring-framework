{
  if (obj == this) {
    return true;
  }
  if (!(obj instanceof ParameterAnnotation<?>)) {
    return false;
  }
  ParameterAnnotation<?> that=(ParameterAnnotation<?>)obj;
  return this.annotation.equals(that.annotation) && this.parameterType.equals(that.parameterType) && this.parameterIndex == that.parameterIndex;
}
