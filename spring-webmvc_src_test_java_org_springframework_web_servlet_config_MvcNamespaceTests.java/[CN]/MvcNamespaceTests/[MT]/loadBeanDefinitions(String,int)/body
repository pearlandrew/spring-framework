{
  XmlBeanDefinitionReader reader=new XmlBeanDefinitionReader(appContext);
  ClassPathResource resource=new ClassPathResource(fileName,AnnotationDrivenBeanDefinitionParserTests.class);
  reader.loadBeanDefinitions(resource);
  assertEquals("Bean names: " + Arrays.toString(this.appContext.getBeanDefinitionNames()),expectedBeanCount,appContext.getBeanDefinitionCount());
  appContext.refresh();
}
