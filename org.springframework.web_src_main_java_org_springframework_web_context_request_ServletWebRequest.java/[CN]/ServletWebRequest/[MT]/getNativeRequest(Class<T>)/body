{
  if (requiredType != null) {
    ServletRequest request=getRequest();
    while (request != null) {
      if (requiredType.isInstance(request)) {
        return (T)request;
      }
 else       if (request instanceof ServletRequestWrapper) {
        request=((ServletRequestWrapper)request).getRequest();
      }
 else {
        request=null;
      }
    }
  }
  return null;
}
