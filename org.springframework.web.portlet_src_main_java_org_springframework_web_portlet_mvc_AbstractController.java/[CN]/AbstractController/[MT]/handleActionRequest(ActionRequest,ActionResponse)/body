{
  check(request,response);
  if (this.synchronizeOnSession) {
    PortletSession session=request.getPortletSession(false);
    if (session != null) {
      Object mutex=PortletUtils.getSessionMutex(session);
synchronized (mutex) {
        handleActionRequestInternal(request,response);
        return;
      }
    }
  }
  handleActionRequestInternal(request,response);
}
