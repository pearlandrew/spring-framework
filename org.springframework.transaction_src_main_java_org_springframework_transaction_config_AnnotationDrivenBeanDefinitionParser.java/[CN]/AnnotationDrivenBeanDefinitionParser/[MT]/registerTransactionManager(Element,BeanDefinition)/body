{
  String transactionManagerName=(element.hasAttribute(TxNamespaceUtils.TRANSACTION_MANAGER_ATTRIBUTE) ? element.getAttribute(TxNamespaceUtils.TRANSACTION_MANAGER_ATTRIBUTE) : DEFAULT_TRANSACTION_MANAGER_BEAN_NAME);
  def.getPropertyValues().addPropertyValue(TxNamespaceUtils.TRANSACTION_MANAGER_PROPERTY,new RuntimeBeanReference(transactionManagerName));
}
