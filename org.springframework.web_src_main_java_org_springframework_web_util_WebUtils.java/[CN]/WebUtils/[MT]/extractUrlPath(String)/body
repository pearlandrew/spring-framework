{
  int end=url.indexOf('?');
  if (end == -1) {
    end=url.indexOf('#');
    if (end == -1) {
      end=url.length();
    }
  }
  url=url.substring(0,end);
  int start=url.indexOf("://");
  if (start != -1) {
    start=url.indexOf('/',start + 3);
    url=(start != -1) ? url.substring(start) : "";
  }
  return url;
}
