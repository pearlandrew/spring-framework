{
  int end=urlPath.indexOf(';');
  if (end == -1) {
    end=urlPath.indexOf('?');
    if (end == -1) {
      end=urlPath.length();
    }
  }
  int begin=urlPath.lastIndexOf('/',end) + 1;
  String filename=urlPath.substring(begin,end);
  int dotIndex=filename.lastIndexOf('.');
  if (dotIndex != -1) {
    filename=filename.substring(0,dotIndex);
  }
  return filename;
}
