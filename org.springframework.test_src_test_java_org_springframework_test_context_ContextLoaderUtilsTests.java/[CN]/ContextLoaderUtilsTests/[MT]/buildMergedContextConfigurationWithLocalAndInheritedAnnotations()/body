{
  Class<?> testClass=Bar.class;
  MergedContextConfiguration mergedConfig=ContextLoaderUtils.buildMergedContextConfiguration(testClass,null);
  String[] expectedLocations=new String[]{"org.springframework.test.context.ContextLoaderUtilsTests$FooConfig","org.springframework.test.context.ContextLoaderUtilsTests$BarConfig"};
  assertMergedContextConfiguration(mergedConfig,testClass,expectedLocations,AnnotationConfigContextLoader.class);
}
