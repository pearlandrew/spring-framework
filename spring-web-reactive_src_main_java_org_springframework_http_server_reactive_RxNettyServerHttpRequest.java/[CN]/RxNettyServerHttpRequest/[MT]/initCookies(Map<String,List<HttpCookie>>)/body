{
  for (  String name : this.request.getCookies().keySet()) {
    List<HttpCookie> list=map.get(name);
    if (list == null) {
      list=new ArrayList<>();
      map.put(name,list);
    }
    for (    Cookie cookie : this.request.getCookies().get(name)) {
      list.add(HttpCookie.clientCookie(name,cookie.value()));
    }
  }
}
