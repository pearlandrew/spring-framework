{
  int index=firstResourceElement.lastIndexOf("/");
  String dir;
  if (index == -1) {
    dir=null;
  }
 else {
    dir=firstResourceElement.substring(0,index + 1);
  }
  String filename=firstResourceElement.substring(index + 1,firstResourceElement.length());
  return new String[]{dir,filename};
}
