{
  for (  Resource resource : resources) {
    InputStream in=null;
    try {
      in=resource.getInputStream();
      int bytesRead=-1;
      byte[] buffer=new byte[4096];
      while ((bytesRead=in.read(buffer)) != -1) {
        response.getOutputStream().write(buffer,0,bytesRead);
      }
    }
 catch (    IOException e) {
    }
 finally {
      if (in != null) {
        try {
          in.close();
        }
 catch (        IOException e) {
        }
      }
    }
  }
}
