{
  DefaultTypeConverter converter=new DefaultTypeConverter();
  MapToMap c=new MapToMap(new TypeDescriptor<Map<String,String>>(getClass().getField("source")),new TypeDescriptor<Map<String,FooEnum>>(getClass().getField("bindTarget")),converter);
  source.put("1","BAR");
  source.put("2","BAZ");
  Map<String,FooEnum> result=(Map<String,FooEnum>)c.execute(source);
  assertEquals(FooEnum.BAR,result.get(1));
  assertEquals(FooEnum.BAZ,result.get(2));
}
