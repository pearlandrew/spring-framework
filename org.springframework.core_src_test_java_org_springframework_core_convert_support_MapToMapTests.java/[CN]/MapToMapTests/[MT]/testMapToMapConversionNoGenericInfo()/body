{
  DefaultTypeConverter service=new DefaultTypeConverter();
  MapToMap c=new MapToMap(ConversionPoint.valueOf(Map.class),ConversionPoint.valueOf(Map.class),service);
  source.put("1","BAR");
  source.put("2","BAZ");
  Map result=(Map)c.execute(source);
  assertEquals("BAR",result.get("1"));
  assertEquals("BAZ",result.get("2"));
}
