{
  WebAppConfiguration webAppConfiguration=AnnotationUtils.findAnnotation(testClass,WebAppConfiguration.class);
  if (webAppConfiguration != null) {
    String resourceBasePath=webAppConfiguration.value();
    return new WebMergedContextConfiguration(testClass,locations,classes,initializerClasses,activeProfiles,propertySourceLocations,propertySourceProperties,resourceBasePath,contextLoader,cacheAwareContextLoaderDelegate,parentConfig);
  }
  return super.buildMergedContextConfiguration(testClass,locations,classes,initializerClasses,activeProfiles,propertySourceLocations,propertySourceProperties,contextLoader,cacheAwareContextLoaderDelegate,parentConfig);
}
