{
  String destination=SimpMessageHeaderAccessor.getDestination(message.getHeaders());
  DestinationInfo info=parseUserDestination(message);
  if (info == null) {
    return null;
  }
  Set<String> targetDestinations=new HashSet<String>();
  for (  String sessionId : info.getSessionIds()) {
    targetDestinations.add(getTargetDestination(destination,info.getDestinationWithoutPrefix(),sessionId,info.getUser()));
  }
  return new UserDestinationResult(destination,targetDestinations,info.getSubscribeDestination(),info.getUser());
}
