{
  assertEquals(3,is.available());
  int ch=is.read();
  assertEquals('a',ch);
  ch=is.read();
  assertEquals('b',ch);
  ch=is.read();
  assertEquals('c',ch);
  assertEquals(2,is.available());
  ch=is.read();
  assertEquals('d',ch);
  ch=is.read();
  assertEquals('e',ch);
  ch=is.read();
  assertEquals(-1,ch);
  assertEquals(0,is.available());
}
