{
  txContext.startTransaction();
  ++this.transactionsStarted;
  if (logger.isInfoEnabled()) {
    logger.info(String.format("Began transaction (%s) for test context %s; transaction manager [%s]; rollback [%s]",this.transactionsStarted,testContext,txContext.transactionManager,isRollback(testContext)));
  }
}
