{
  return Boolean.TRUE.equals(process(element,annotationType,true,false,new Processor<Boolean>(){
    @Override public Boolean process(    Annotation annotation,    int metaDepth){
      if (metaDepth > 0) {
        return Boolean.TRUE;
      }
      return null;
    }
