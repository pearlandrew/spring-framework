{
  for (  Class<?> iface : ifcs) {
    if (AnnotationUtils.isInterfaceWithAnnotatedMethods(iface)) {
      try {
        Method equivalentMethod=iface.getMethod(method.getName(),method.getParameterTypes());
        T result=searchWithFindSemantics(equivalentMethod,annotationName,searchOnInterfaces,searchOnSuperclasses,searchOnMethodsInInterfaces,searchOnMethodsInSuperclasses,processor,visited,metaDepth);
        if (result != null) {
          return result;
        }
      }
 catch (      NoSuchMethodException ex) {
      }
    }
  }
  return null;
}
