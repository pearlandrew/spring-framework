{
  return new RequestMappingInfo(new PatternsRequestCondition(annot.value(),getUrlPathHelper(),getPathMatcher(),useSuffixPatternMatch),new RequestMethodsRequestCondition(annot.method()),new ParamsRequestCondition(annot.params()),new HeadersRequestCondition(annot.headers()),new ConsumesRequestCondition(annot.consumes(),annot.headers()),new ProducesRequestCondition(annot.produces(),annot.headers()),null);
}
