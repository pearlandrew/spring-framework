{
  Class handlerClass=(handler instanceof String ? getApplicationContext().getType((String)handler) : handler.getClass());
  RequestMapping mapping=this.cachedMappings.get(handlerClass);
  if (mapping == null) {
    mapping=AnnotationUtils.findAnnotation(handlerClass,RequestMapping.class);
  }
  if (mapping != null) {
    validateMapping(mapping,request);
  }
}
