{
  if (source == null) {
    return null;
  }
  if (source.getClass().isAssignableFrom(targetType.getType())) {
    return source;
  }
  ConversionExecutor executor=getConversionExecutor(source.getClass(),targetType);
  if (executor != null) {
    return executor.execute(source);
  }
 else {
    if (parent != null) {
      return parent.convert(source,targetType);
    }
 else {
      throw new ConverterNotFoundException(source.getClass(),targetType.getType(),"No converter found that can convert from sourceType [" + source.getClass().getName() + "] to targetType ["+ targetType.getName()+ "]");
    }
  }
}
