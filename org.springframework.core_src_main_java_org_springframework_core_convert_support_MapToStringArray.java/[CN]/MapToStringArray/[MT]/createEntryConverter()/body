{
  if (targetType.isMapEntryTypeKnown()) {
    ConversionExecutor keyConverter=conversionService.getConversionExecutor(targetType.getMapKeyType(),TypeDescriptor.valueOf(String.class));
    ConversionExecutor valueConverter=conversionService.getConversionExecutor(targetType.getMapValueType(),TypeDescriptor.valueOf(String.class));
    return new MapEntryConverter(keyConverter,valueConverter);
  }
 else {
    return MapEntryConverter.NO_OP_INSTANCE;
  }
}
