{
  Flux<ByteBuffer> source=Flux.just(Buffer.wrap("foo").byteBuffer(),Buffer.wrap("bar").byteBuffer());
  Single<String> single=RxJava1SingleConverter.from(this.decoder.decode(source,ResolvableType.forClassWithGenerics(Single.class,String.class),MediaType.TEXT_PLAIN));
  String result=single.toBlocking().value();
  assertEquals("foobar",result);
}
