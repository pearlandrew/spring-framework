{
  StringBuilder s=new StringBuilder();
  s.append('{');
  int count=getChildCount();
  for (int c=0; c < count; c++) {
    if (c > 0) {
      s.append(',');
    }
    s.append(getChild(c++).toStringAST());
    s.append(':');
    s.append(getChild(c).toStringAST());
  }
  s.append('}');
  return s.toString();
}
