{
  MockHttpServletRequest request=new MockHttpServletRequest("GET","/foo");
  request.setParameter("foo","bar");
  RequestMappingInfo key=new RequestMappingInfo(new PatternsRequestCondition("/foo"),null,new ParamsRequestCondition("foo=bar"),null,null,null);
  RequestMappingInfo match=key.getMatchingRequestMapping(request);
  assertNotNull(match);
  key=new RequestMappingInfo(new PatternsRequestCondition("/foo"),null,new ParamsRequestCondition("foo!=bar"),null,null,null);
  match=key.getMatchingRequestMapping(request);
  assertNull(match);
}
