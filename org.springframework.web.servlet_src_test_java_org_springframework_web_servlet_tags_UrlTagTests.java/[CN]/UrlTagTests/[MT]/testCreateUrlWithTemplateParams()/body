{
  tag.setValue("url/{name}");
  tag.doStartTag();
  Param param=new Param();
  param.setName("name");
  param.setValue("value");
  tag.addParam(param);
  param=new Param();
  param.setName("n me");
  param.setValue("v lue");
  tag.addParam(param);
  String uri=invokeCreateUrl(tag);
  assertEquals("url/value?n+me=v+lue",uri);
}
