{
  Object left=getLeftOperand().getValue(state);
  Object right=getRightOperand().getValue(state);
  if (right instanceof Collection<?>) {
    Collection<?> c=(Collection<?>)right;
    for (    Object element : c) {
      if (state.getTypeComparator().compare(left,element) == 0) {
        return true;
      }
    }
    return false;
  }
  throw new SpelException(SpelMessages.OPERATOR_IN_CANNOT_DETERMINE_MEMBERSHIP,left.getClass().getName(),right.getClass().getName());
}
