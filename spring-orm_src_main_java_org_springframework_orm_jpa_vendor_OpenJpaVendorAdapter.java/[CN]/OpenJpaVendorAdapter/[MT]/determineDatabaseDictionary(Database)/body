{
switch (database) {
case DB2:
    return "db2";
case DERBY:
  return "derby";
case HSQL:
return "hsql(SimulateLocking=true)";
case INFORMIX:
return "informix";
case MYSQL:
return "mysql";
case ORACLE:
return "oracle";
case POSTGRESQL:
return "postgres";
case SQL_SERVER:
return "sqlserver";
case SYBASE:
return "sybase";
default :
return null;
}
}
