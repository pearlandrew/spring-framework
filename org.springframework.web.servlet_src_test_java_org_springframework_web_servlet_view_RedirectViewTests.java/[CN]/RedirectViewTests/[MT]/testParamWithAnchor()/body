{
  String url="http://url.somewhere.com/test.htm#myAnchor";
  String key="foo";
  String val="bar";
  Map model=new HashMap();
  model.put(key,val);
  String expectedUrlForEncoding="http://url.somewhere.com/test.htm" + "?" + key + "="+ val+ "#myAnchor";
  doTest(model,url,false,expectedUrlForEncoding);
}
