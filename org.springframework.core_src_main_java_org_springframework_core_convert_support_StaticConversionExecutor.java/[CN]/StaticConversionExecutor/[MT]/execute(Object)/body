{
  if (source == null) {
    return null;
  }
  if (sourceType != null && !sourceType.isInstance(source)) {
    throw new ConversionFailedException(source,sourceType.getType(),targetType.getType(),"Source object " + source + " to convert is expected to be an instance of ["+ sourceType.getName()+ "]");
  }
  try {
    return converter.convert(source);
  }
 catch (  Exception e) {
    throw new ConversionFailedException(source,sourceType.getType(),targetType.getType(),e);
  }
}
