{
  if (source == null) {
    return null;
  }
  if (!this.sourceType.isInstance(source)) {
    throw new ConversionFailedException(source,sourceType.getType(),targetType.getType(),"Source object " + source + " to convert is expected to be an instance of ["+ sourceType.getName()+ "]");
  }
  try {
    return this.converter.convert(source);
  }
 catch (  Exception ex) {
    throw new ConversionFailedException(source,sourceType.getType(),targetType.getType(),ex);
  }
}
