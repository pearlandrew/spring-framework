{
  if (this.fileTypeMap == null) {
    try {
      this.fileTypeMap=createFileTypeMap(this.mappingLocation,this.mappings);
    }
 catch (    IOException ex) {
      IllegalStateException ise=new IllegalStateException("Could not load specified MIME type mapping file: " + this.mappingLocation);
      ise.initCause(ex);
      throw ise;
    }
  }
  return this.fileTypeMap;
}
