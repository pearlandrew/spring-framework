{
synchronized (this.lifecycleMonitor) {
    if (logger.isInfoEnabled()) {
      logger.info("Stopping...");
    }
    stopInternal();
    this.clientInboundChannel.unsubscribe(this);
    this.brokerChannel.unsubscribe(this);
    this.running=false;
    if (logger.isDebugEnabled()) {
      logger.info("Stopped.");
    }
  }
}
