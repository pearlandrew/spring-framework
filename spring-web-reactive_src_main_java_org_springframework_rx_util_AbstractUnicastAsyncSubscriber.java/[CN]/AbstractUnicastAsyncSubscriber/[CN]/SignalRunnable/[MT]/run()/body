{
  if (enabled.get()) {
    try {
      Signal<T> s=inboundSignals.poll();
      if (!done) {
        if (s.isOnNext()) {
          handleOnNext(s.next());
        }
 else         if (s.isOnSubscribe()) {
          handleOnSubscribe(s.subscription());
        }
 else         if (s.isOnError()) {
          handleOnError(s.error());
        }
 else         if (s.isComplete()) {
          handleOnComplete();
        }
      }
    }
  finally {
      enabled.set(false);
      if (!inboundSignals.isEmpty()) {
        tryScheduleToExecute();
      }
    }
  }
}
