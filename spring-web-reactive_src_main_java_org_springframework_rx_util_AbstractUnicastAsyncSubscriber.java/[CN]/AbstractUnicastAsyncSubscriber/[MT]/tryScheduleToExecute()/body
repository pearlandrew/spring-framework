{
  if (enabled.compareAndSet(false,true)) {
    try {
      executor.execute(new SignalRunnable());
    }
 catch (    Throwable t) {
      if (!done) {
        try {
          done();
        }
  finally {
          inboundSignals.clear();
          enabled.set(false);
        }
      }
    }
  }
}
