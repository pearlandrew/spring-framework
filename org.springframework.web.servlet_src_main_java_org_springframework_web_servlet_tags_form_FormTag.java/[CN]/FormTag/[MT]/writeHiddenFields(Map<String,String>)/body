{
  if (hiddenFields != null) {
    for (    String name : hiddenFields.keySet()) {
      this.tagWriter.appendValue("<input type=\"hidden\" ");
      this.tagWriter.appendValue("name=\"" + name + "\" value=\""+ hiddenFields.get(name)+ "\" ");
      this.tagWriter.appendValue("</input>\n");
    }
  }
}
