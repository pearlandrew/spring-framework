{
  JmsListenerContainerTestFactory defaultFactory=context.getBean("defaultFactory",JmsListenerContainerTestFactory.class);
  JmsListenerContainerTestFactory simpleFactory=context.getBean("simpleFactory",JmsListenerContainerTestFactory.class);
  assertEquals(1,defaultFactory.getContainers().size());
  assertEquals(1,simpleFactory.getContainers().size());
}
