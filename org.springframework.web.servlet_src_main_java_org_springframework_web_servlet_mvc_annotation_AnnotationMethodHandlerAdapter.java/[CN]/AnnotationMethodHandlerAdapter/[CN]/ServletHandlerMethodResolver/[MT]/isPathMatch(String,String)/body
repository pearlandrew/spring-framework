{
  if (mappedPath.equals(lookupPath) || pathMatcher.match(mappedPath,lookupPath)) {
    return true;
  }
  boolean hasSuffix=(mappedPath.indexOf('.') != -1);
  if (!hasSuffix && pathMatcher.match(mappedPath + ".*",lookupPath)) {
    return true;
  }
  return (!mappedPath.startsWith("/") && (lookupPath.endsWith(mappedPath) || pathMatcher.match("/**/" + mappedPath,lookupPath) || (!hasSuffix && pathMatcher.match("/**/" + mappedPath + ".*",lookupPath))));
}
