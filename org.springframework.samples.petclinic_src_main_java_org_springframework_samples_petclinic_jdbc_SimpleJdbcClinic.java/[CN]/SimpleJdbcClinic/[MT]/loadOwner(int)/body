{
  Owner owner;
  try {
    owner=this.simpleJdbcTemplate.queryForObject("SELECT id, first_name, last_name, address, city, telephone FROM owners WHERE id=?",ParameterizedBeanPropertyRowMapper.newInstance(Owner.class),id);
  }
 catch (  EmptyResultDataAccessException ex) {
    throw new ObjectRetrievalFailureException(Owner.class,new Integer(id));
  }
  loadPetsAndVisits(owner);
  return owner;
}
