{
  MultiValueMap<String,Object> attributesMultiMap=AnnotatedElementUtils.getAllAnnotationAttributes(testClass,BootstrapWith.class.getName());
  List<Object> values=(attributesMultiMap != null ? attributesMultiMap.get(AnnotationUtils.VALUE) : null);
  if (values == null) {
    return null;
  }
  if (values.size() != 1) {
    throw new IllegalStateException(String.format("Configuration error: found multiple declarations of " + "@BootstrapWith on test class [%s] with values %s",testClass.getName(),values));
  }
  return (Class<?>)values.get(0);
}
