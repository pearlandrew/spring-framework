{
  MimeType mimeType=getMimeType(headers);
  if (mimeType == null) {
    return true;
  }
  if (getSupportedMimeTypes().isEmpty()) {
    return true;
  }
  for (  MimeType supported : getSupportedMimeTypes()) {
    if (supported.getType().equals(mimeType.getType()) && supported.getSubtype().equals(mimeType.getSubtype())) {
      return true;
    }
  }
  return false;
}
