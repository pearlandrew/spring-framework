{
  if (logger.isInfoEnabled()) {
    logger.info("Enhancing " + configClass.getName());
  }
  Class<?> enhancedClass=createClass(newEnhancer(configClass));
  if (logger.isInfoEnabled()) {
    logger.info(String.format("Successfully enhanced %s; enhanced class name is: %s",configClass.getName(),enhancedClass.getName()));
  }
  return enhancedClass;
}
