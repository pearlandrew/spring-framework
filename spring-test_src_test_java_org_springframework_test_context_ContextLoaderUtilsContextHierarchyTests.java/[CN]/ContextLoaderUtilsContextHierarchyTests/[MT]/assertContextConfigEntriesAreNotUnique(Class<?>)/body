{
  try {
    resolveContextHierarchyAttributes(testClass);
    fail("Should throw an IllegalStateException");
  }
 catch (  IllegalStateException e) {
    String msg=String.format("The @ContextConfiguration elements configured via @ContextHierarchy in test class [%s] must define unique contexts to load.",testClass.getName());
    assertEquals(msg,e.getMessage());
  }
}
