{
  this.mockMvc.perform(get("/handle")).andExpect(content().mimeType(MediaType.TEXT_PLAIN)).andExpect(content().mimeType("text/plain"));
  this.mockMvc.perform(get("/handleUtf8")).andExpect(content().mimeType(MediaType.valueOf("text/plain;charset=UTF-8"))).andExpect(content().mimeType("text/plain;charset=UTF-8"));
}
