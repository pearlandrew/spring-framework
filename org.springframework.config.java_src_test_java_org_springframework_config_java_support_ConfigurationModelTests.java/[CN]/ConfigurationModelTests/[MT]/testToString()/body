{
  ConfigurationModel model=new ConfigurationModel();
  assertThat(model.toString(),equalTo("ConfigurationModel containing @Configuration classes: []"));
  ConfigurationClass config1=new ConfigurationClass();
  config1.setName("test.Config1");
  model.add(config1);
  assertThat(model.toString(),equalTo(format("ConfigurationModel containing @Configuration classes: [%s]",config1)));
}
