{
  Assert.hasLength(name,"'name' is required and must not be empty.");
  Assert.hasLength(value,"'value' is required and must not be empty.");
  this.name=name;
  this.value=value;
  this.maxAge=(maxAge > -1 ? maxAge : -1);
  this.domain=domain;
  this.path=path;
  this.secure=secure;
  this.httpOnly=httpOnly;
}
