{
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(InputStreamResource.class),MimeTypeUtils.TEXT_PLAIN,Collections.emptyMap()));
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(ByteArrayResource.class),MimeTypeUtils.TEXT_PLAIN,Collections.emptyMap()));
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(Resource.class),MimeTypeUtils.TEXT_PLAIN,Collections.emptyMap()));
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(InputStreamResource.class),MimeTypeUtils.APPLICATION_JSON,Collections.emptyMap()));
}
