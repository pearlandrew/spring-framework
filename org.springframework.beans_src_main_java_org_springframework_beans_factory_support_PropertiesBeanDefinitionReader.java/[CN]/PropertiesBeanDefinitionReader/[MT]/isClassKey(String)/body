{
  if (CLASS_KEY.equals(property)) {
    return true;
  }
 else   if (DEPRECATED_CLASS_KEY.equals(property)) {
    if (logger.isWarnEnabled()) {
      logger.warn("Use of 'class' property in [" + getClass().getName() + "] is deprecated in favor of '(class)'");
    }
    return true;
  }
  return false;
}
