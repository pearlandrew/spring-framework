{
  Class<?>[] classes=new Class<?>[]{String.class,Integer.class};
  MergedContextConfiguration mergedConfig1=new MergedContextConfiguration(getClass(),EMPTY_STRING_ARRAY,classes,EMPTY_STRING_ARRAY,loader);
  MergedContextConfiguration mergedConfig2=new MergedContextConfiguration(getClass(),EMPTY_STRING_ARRAY,classes,EMPTY_STRING_ARRAY,loader);
  assertEquals(mergedConfig1,mergedConfig2);
}
