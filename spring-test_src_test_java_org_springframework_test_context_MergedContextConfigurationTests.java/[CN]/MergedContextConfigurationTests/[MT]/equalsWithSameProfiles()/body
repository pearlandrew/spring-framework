{
  String[] activeProfiles=new String[]{"catbert","dogbert"};
  MergedContextConfiguration mergedConfig1=new MergedContextConfiguration(getClass(),EMPTY_STRING_ARRAY,EMPTY_CLASS_ARRAY,activeProfiles,loader);
  MergedContextConfiguration mergedConfig2=new MergedContextConfiguration(getClass(),EMPTY_STRING_ARRAY,EMPTY_CLASS_ARRAY,activeProfiles,loader);
  assertEquals(mergedConfig1,mergedConfig2);
}
