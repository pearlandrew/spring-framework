{
  if (!isGzipAccepted(request) || resolved == null) {
    return resolved;
  }
  try {
    Resource gzipped=new GzippedResource(resolved);
    if (gzipped.exists()) {
      return gzipped;
    }
  }
 catch (  IOException e) {
    this.logger.trace("Error occurred locating gzipped resource",e);
  }
  return resolved;
}
