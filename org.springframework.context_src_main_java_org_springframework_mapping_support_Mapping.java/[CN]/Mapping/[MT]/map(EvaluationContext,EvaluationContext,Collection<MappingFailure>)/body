{
  try {
    Object value=this.source.getValue(sourceContext);
    if (this.converter != null) {
      value=this.converter.convert(value,this.source.getValueTypeDescriptor(sourceContext),this.target.getValueTypeDescriptor(targetContext));
    }
    this.target.setValue(targetContext,value);
  }
 catch (  Exception e) {
    failures.add(new MappingFailure(e));
  }
}
