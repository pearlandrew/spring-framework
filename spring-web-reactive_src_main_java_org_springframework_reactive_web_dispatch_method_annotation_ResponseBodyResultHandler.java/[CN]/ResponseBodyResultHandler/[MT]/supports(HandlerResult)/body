{
  Object handler=result.getHandler();
  if (handler instanceof HandlerMethod) {
    HandlerMethod handlerMethod=(HandlerMethod)handler;
    Type publisherVoidType=new ParameterizedTypeReference<Publisher<Void>>(){
    }
.getType();
    return AnnotatedElementUtils.isAnnotated(handlerMethod.getMethod(),ResponseBody.class.getName()) && !handlerMethod.getReturnType().getGenericParameterType().equals(publisherVoidType);
  }
  return false;
}
