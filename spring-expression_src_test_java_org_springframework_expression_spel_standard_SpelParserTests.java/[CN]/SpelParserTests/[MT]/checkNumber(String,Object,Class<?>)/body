{
  try {
    SpelExpressionParser parser=new SpelExpressionParser();
    SpelExpression expr=parser.parseRaw(expression);
    Object o=expr.getValue();
    assertEquals(value,o);
    assertEquals(type,o.getClass());
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
