{
  WebServiceFeature[] wsFeatures=new WebServiceFeature[features.length];
  for (int i=0; i < features.length; i++) {
    wsFeatures[i]=convertWebServiceFeature(features[i]);
  }
  return (portQName != null ? service.getPort(portQName,getServiceInterface(),wsFeatures) : service.getPort(getServiceInterface(),wsFeatures));
}
