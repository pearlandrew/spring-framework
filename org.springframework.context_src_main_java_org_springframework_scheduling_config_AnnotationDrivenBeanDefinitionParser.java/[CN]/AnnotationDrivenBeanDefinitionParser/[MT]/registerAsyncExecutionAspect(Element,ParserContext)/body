{
  if (!parserContext.getRegistry().containsBeanDefinition(AnnotationConfigUtils.ASYNC_EXECUTION_ASPECT_BEAN_NAME)) {
    BeanDefinitionBuilder builder=BeanDefinitionBuilder.genericBeanDefinition(AnnotationConfigUtils.ASYNC_EXECUTION_ASPECT_CLASS_NAME);
    builder.setFactoryMethod("aspectOf");
    String executor=element.getAttribute("executor");
    if (StringUtils.hasText(executor)) {
      builder.addPropertyReference("executor",executor);
    }
    parserContext.registerBeanComponent(new BeanComponentDefinition(builder.getBeanDefinition(),AnnotationConfigUtils.ASYNC_EXECUTION_ASPECT_BEAN_NAME));
  }
}
