{
  logger.error("Closing due to transport error for " + this,ex);
  try {
    delegateError(ex);
  }
 catch (  Throwable delegateEx) {
    logger.error("Unhandled error for " + this,delegateEx);
    try {
      close(closeStatus);
    }
 catch (    Throwable closeEx) {
      logger.error("Unhandled error for " + this,closeEx);
    }
  }
}
