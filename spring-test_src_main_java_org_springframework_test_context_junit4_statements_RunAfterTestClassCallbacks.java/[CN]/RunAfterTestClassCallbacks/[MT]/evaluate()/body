{
  List<Throwable> errors=new ArrayList<Throwable>();
  try {
    this.next.evaluate();
  }
 catch (  Throwable ex) {
    errors.add(ex);
  }
  try {
    this.testContextManager.afterTestClass();
  }
 catch (  Throwable ex) {
    errors.add(ex);
  }
  if (errors.isEmpty()) {
    return;
  }
  if (errors.size() == 1) {
    throw errors.get(0);
  }
  throw new MultipleFailureException(errors);
}
