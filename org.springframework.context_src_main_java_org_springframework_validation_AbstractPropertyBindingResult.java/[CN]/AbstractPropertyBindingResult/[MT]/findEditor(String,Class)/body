{
  TypeDescriptor td=(valueType != null ? TypeDescriptor.valueOf(valueType) : getPropertyAccessor().getPropertyTypeDescriptor(fixedField(field)));
  final Formatter<Object> formatter=(this.formatterRegistry != null ? this.formatterRegistry.getFormatter(td) : null);
  if (formatter != null) {
    return new FormattingPropertyEditorAdapter(formatter);
  }
  return super.findEditor(field,valueType);
}
