{
  if (valueType == null) {
    valueType=getFieldType(field);
  }
  PropertyEditor editor=super.findEditor(field,valueType);
  if (editor == null && this.conversionService != null) {
    TypeDescriptor td=(field != null ? getPropertyAccessor().getPropertyTypeDescriptor(fixedField(field)) : TypeDescriptor.valueOf(valueType));
    if (this.conversionService.canConvert(TypeDescriptor.STRING,td)) {
      editor=new ConvertingPropertyEditorAdapter(this.conversionService,td);
    }
  }
  return editor;
}
