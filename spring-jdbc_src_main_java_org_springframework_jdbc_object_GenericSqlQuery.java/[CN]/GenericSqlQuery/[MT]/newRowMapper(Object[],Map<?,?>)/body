{
  try {
    return (RowMapper<T>)rowMapperClass.newInstance();
  }
 catch (  InstantiationException e) {
    throw new InvalidDataAccessResourceUsageException("Unable to instantiate RowMapper",e);
  }
catch (  IllegalAccessException e) {
    throw new InvalidDataAccessResourceUsageException("Unable to instantiate RowMapper",e);
  }
}
