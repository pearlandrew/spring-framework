{
  Class<? extends Annotation>[] classesToLookFor=new Class[]{Before.class,Around.class,After.class,AfterReturning.class,AfterThrowing.class,Pointcut.class};
  for (  Class<? extends Annotation> c : classesToLookFor) {
    AspectJAnnotation foundAnnotation=findAnnotation(method,c);
    if (foundAnnotation != null) {
      return foundAnnotation;
    }
  }
  return null;
}
