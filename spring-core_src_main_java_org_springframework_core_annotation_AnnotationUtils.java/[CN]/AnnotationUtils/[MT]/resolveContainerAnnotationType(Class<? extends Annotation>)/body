{
  try {
    Annotation repeatable=getAnnotation(annotationType,REPEATABLE_CLASS_NAME);
    if (repeatable != null) {
      Object value=getValue(repeatable);
      return (Class<? extends Annotation>)value;
    }
  }
 catch (  Exception ex) {
    handleIntrospectionFailure(annotationType,ex);
  }
  return null;
}
