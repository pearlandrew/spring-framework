{
  Assert.notNull(annotationType,"annotationType must not be null");
  if (attributes == null) {
    return null;
  }
  MapAnnotationAttributeExtractor attributeExtractor=new MapAnnotationAttributeExtractor(attributes,annotationType,annotatedElement);
  InvocationHandler handler=new SynthesizedAnnotationInvocationHandler(attributeExtractor);
  A synthesizedAnnotation=(A)Proxy.newProxyInstance(ClassUtils.getDefaultClassLoader(),new Class<?>[]{annotationType,SynthesizedAnnotation.class},handler);
  return synthesizedAnnotation;
}
