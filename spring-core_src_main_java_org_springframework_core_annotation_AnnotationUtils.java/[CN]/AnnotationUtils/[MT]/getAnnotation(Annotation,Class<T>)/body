{
  if (annotationType.isInstance(ann)) {
    return (T)ann;
  }
  try {
    return ann.annotationType().getAnnotation(annotationType);
  }
 catch (  Exception ex) {
    if (logger.isInfoEnabled()) {
      logger.info("Failed to introspect annotations on [" + ann.annotationType() + "]: "+ ex);
    }
    return null;
  }
}
