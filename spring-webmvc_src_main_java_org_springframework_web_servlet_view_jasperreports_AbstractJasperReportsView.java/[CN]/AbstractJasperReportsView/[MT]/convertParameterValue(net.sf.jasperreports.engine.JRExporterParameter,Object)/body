{
  if (value instanceof String) {
    String str=(String)value;
    if ("true".equals(str)) {
      return Boolean.TRUE;
    }
 else     if ("false".equals(str)) {
      return Boolean.FALSE;
    }
 else     if (str.length() > 0 && Character.isDigit(str.charAt(0))) {
      try {
        return new Integer(str);
      }
 catch (      NumberFormatException ex) {
        return str;
      }
    }
  }
  return value;
}
