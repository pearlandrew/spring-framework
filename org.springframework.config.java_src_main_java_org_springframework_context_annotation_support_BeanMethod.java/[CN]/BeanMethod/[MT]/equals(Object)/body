{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  BeanMethod other=(BeanMethod)obj;
  if (annotations == null) {
    if (other.annotations != null)     return false;
  }
 else   if (!annotations.equals(other.annotations))   return false;
  if (modifiers != other.modifiers)   return false;
  if (name == null) {
    if (other.name != null)     return false;
  }
 else   if (!name.equals(other.name))   return false;
  if (returnType == null) {
    if (other.returnType != null)     return false;
  }
 else   if (!returnType.equals(other.returnType))   return false;
  return true;
}
