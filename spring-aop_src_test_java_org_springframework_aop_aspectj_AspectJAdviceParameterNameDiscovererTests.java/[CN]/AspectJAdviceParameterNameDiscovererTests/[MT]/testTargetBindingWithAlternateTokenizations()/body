{
  assertParameterNames(getMethod("oneObject"),"target( x )",new String[]{"x"});
  assertParameterNames(getMethod("oneObject"),"target( x)",new String[]{"x"});
  assertParameterNames(getMethod("oneObject"),"target (x )",new String[]{"x"});
  assertParameterNames(getMethod("oneObject"),"target(x )",new String[]{"x"});
  assertParameterNames(getMethod("oneObject"),"foo() && target(x)",new String[]{"x"});
}
