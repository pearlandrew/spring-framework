{
  DefaultConversionService service=new DefaultConversionService();
  MapToMap c=new MapToMap(new TypeDescriptor(getClass().getField("source")),new TypeDescriptor(getClass().getField("bindTarget")),service);
  source.put("1","BAR");
  source.put("2","BAZ");
  Map result=(Map)c.execute(source);
  assertEquals(FooEnum.BAR,result.get(1));
  assertEquals(FooEnum.BAZ,result.get(2));
}
