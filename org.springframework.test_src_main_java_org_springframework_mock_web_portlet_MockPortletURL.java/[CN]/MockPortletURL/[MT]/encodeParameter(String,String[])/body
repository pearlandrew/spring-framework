{
  try {
    StringBuffer buf=new StringBuffer();
    for (int i=0, n=values.length; i < n; i++) {
      buf.append((i > 0 ? ";" : "") + URLEncoder.encode(name,ENCODING) + "="+ URLEncoder.encode(values[i],ENCODING));
    }
    return buf.toString();
  }
 catch (  UnsupportedEncodingException ex) {
    return null;
  }
}
