{
  if (session != null) {
    logger.debug("Closing TopLink Session");
    try {
      session.release();
    }
 catch (    TopLinkException ex) {
      logger.debug("Could not close TopLink Session",ex);
    }
catch (    Throwable ex) {
      logger.debug("Unexpected exception on closing TopLink Session",ex);
    }
  }
}
