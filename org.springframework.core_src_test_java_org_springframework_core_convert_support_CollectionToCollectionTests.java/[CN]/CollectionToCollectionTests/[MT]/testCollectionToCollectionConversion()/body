{
  DefaultTypeConverter service=new DefaultTypeConverter();
  CollectionToCollection c=new CollectionToCollection(new ConversionContext(getClass().getField("bindTarget")),new ConversionContext(getClass().getField("integerTarget")),service);
  bindTarget.add("1");
  bindTarget.add("2");
  bindTarget.add("3");
  List result=(List)c.execute(bindTarget);
  assertEquals(new Integer(1),result.get(0));
  assertEquals(new Integer(2),result.get(1));
  assertEquals(new Integer(3),result.get(2));
}
