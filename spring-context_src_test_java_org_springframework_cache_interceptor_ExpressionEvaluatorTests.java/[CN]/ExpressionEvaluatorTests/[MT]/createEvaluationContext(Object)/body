{
  AnnotatedClass target=new AnnotatedClass();
  Method method=ReflectionUtils.findMethod(AnnotatedClass.class,"multipleCaching",Object.class,Object.class);
  Object[] args=new Object[]{new Object(),new Object()};
  Collection<ConcurrentMapCache> caches=Collections.singleton(new ConcurrentMapCache("test"));
  EvaluationContext context=eval.createEvaluationContext(caches,method,args,target,target.getClass(),result);
  return context;
}
