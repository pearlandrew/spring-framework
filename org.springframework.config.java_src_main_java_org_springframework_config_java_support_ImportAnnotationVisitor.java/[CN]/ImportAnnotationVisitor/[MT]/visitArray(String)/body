{
  Assert.isTrue("value".equals(attribName),format("expected 'value' attribute, got unknown '%s' attribute",attribName));
  return new AnnotationAdapter(AsmUtils.EMPTY_VISITOR){
    @Override public void visit(    String na,    Object type){
      Assert.isInstanceOf(Type.class,type);
      classesToImport.add(((Type)type).getClassName());
    }
  }
;
}
