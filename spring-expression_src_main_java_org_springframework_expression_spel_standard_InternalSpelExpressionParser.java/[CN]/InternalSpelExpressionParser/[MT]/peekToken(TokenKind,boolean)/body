{
  if (!moreTokens()) {
    return false;
  }
  Token t=peekToken();
  if (t.kind == desiredTokenKind) {
    if (consumeIfMatched) {
      tokenStreamPointer++;
    }
    return true;
  }
 else {
    if (desiredTokenKind == TokenKind.IDENTIFIER) {
      if (t.kind.ordinal() >= TokenKind.DIV.ordinal() && t.kind.ordinal() <= TokenKind.NOT.ordinal() && t.data != null) {
        return true;
      }
    }
    return false;
  }
}
