{
  try {
    return handlerMethod.invoke(message,jmsMessage,session);
  }
 catch (  MessagingException e) {
    throw new ListenerExecutionFailedException(createMessagingErrorMessage("Listener method could not " + "be invoked with the incoming message"),e);
  }
catch (  Exception e) {
    throw new ListenerExecutionFailedException("Listener method '" + handlerMethod.getMethod().toGenericString() + "' threw exception",e);
  }
}
