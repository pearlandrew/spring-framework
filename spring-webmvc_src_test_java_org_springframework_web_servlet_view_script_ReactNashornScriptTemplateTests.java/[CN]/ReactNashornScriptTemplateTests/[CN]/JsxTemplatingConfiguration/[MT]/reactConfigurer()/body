{
  ScriptTemplateConfigurer configurer=new ScriptTemplateConfigurer();
  configurer.setEngineName("nashorn");
  configurer.setScripts("org/springframework/web/servlet/view/script/react/polyfill.js","/META-INF/resources/webjars/react/0.12.2/react.js","/META-INF/resources/webjars/react/0.12.2/JSXTransformer.js","org/springframework/web/servlet/view/script/react/render.js");
  configurer.setRenderFunction("renderJsx");
  return configurer;
}
