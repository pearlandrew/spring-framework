{
  if (event instanceof RequestHandledEvent) {
    RequestHandledEvent rhe=(RequestHandledEvent)event;
    this.responseTimeMonitor.recordResponseTime(rhe.getProcessingTimeMillis());
    if (logger.isTraceEnabled()) {
      logger.trace("PerformanceMonitorListener: last=[" + rhe.getProcessingTimeMillis() + "ms]; "+ this.responseTimeMonitor+ "; "+ rhe.getShortDescription());
    }
  }
}
