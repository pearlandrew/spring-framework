{
  if (this.urlDecode) {
    return vars;
  }
 else {
    Map<String,String> decodedVars=new LinkedHashMap<String,String>(vars.size());
    for (    Entry<String,String> entry : vars.entrySet()) {
      decodedVars.put(entry.getKey(),decodeInternal(request,entry.getValue()));
    }
    return decodedVars;
  }
}
