{
  boolean brokerRelayConfigured=getMessageBrokerConfigurer().getStompBrokerRelay() != null;
  ServletStompEndpointRegistry registry=new ServletStompEndpointRegistry(subProtocolWebSocketHandler(),userQueueSuffixResolver(),brokerDefaultSockJsTaskScheduler(),!brokerRelayConfigured);
  registerStompEndpoints(registry);
  AbstractHandlerMapping hm=registry.getHandlerMapping();
  hm.setOrder(1);
  return hm;
}
