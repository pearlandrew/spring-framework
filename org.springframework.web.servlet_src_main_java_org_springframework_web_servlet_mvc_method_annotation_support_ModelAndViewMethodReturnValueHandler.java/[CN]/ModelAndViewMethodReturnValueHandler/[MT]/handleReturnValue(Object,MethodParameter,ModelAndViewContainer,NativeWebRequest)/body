{
  if (returnValue != null) {
    ModelAndView mav=(ModelAndView)returnValue;
    mavContainer.setViewName(mav.getViewName());
    if (!mav.isReference()) {
      mavContainer.setView(mav.getView());
    }
    mavContainer.addAllAttributes(mav.getModel());
  }
 else {
    mavContainer.setResolveView(false);
  }
}
