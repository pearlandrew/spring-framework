{
synchronized (this.lifecycleMonitor) {
    if (isRunning()) {
      try {
        if (logger.isInfoEnabled()) {
          logger.info("Stopping Jetty WebSocketClient");
        }
        this.client.stop();
      }
 catch (      Exception e) {
        logger.error("Error stopping Jetty WebSocketClient",e);
      }
    }
  }
}
