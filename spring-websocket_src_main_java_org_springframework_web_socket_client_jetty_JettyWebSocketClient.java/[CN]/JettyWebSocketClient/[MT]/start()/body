{
synchronized (this.lifecycleMonitor) {
    if (!isRunning()) {
      try {
        if (logger.isInfoEnabled()) {
          logger.info("Starting Jetty WebSocketClient");
        }
        this.client.start();
      }
 catch (      Exception e) {
        throw new IllegalStateException("Failed to start Jetty client",e);
      }
    }
  }
}
