{
  if (INSTANCE == null) {
    System.setProperty("derby.stream.error.method",DerbyEmbeddedDatabaseConfigurer.class.getName() + ".getNoopOutputStream");
    ClassUtils.forName("org.apache.derby.jdbc.EmbeddedDriver",DerbyEmbeddedDatabaseConfigurer.class.getClassLoader());
    INSTANCE=new DerbyEmbeddedDatabaseConfigurer();
  }
  return INSTANCE;
}
