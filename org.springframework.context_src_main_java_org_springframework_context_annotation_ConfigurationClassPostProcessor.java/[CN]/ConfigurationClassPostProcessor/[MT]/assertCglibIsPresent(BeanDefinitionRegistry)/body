{
  try {
    Class.forName(CGLIB_TEST_CLASS);
  }
 catch (  ClassNotFoundException e) {
    throw new IllegalStateException("CGLIB is required to process @Configuration classes. " + "Either add CGLIB to the classpath or remove the following @Configuration bean definitions: [" + StringUtils.arrayToCommaDelimitedString(configBeanDefs.getBeanDefinitionNames()) + "]");
  }
}
