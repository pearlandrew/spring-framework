{
  HandlerMethod existing=this.mappingLookup.get(mapping);
  if (existing != null && !existing.equals(handlerMethod)) {
    throw new IllegalStateException("Ambiguous mapping. Cannot map '" + handlerMethod.getBean() + "' method \n"+ handlerMethod+ "\nto "+ mapping+ ": There is already '"+ existing.getBean()+ "' bean method\n"+ existing+ " mapped.");
  }
}
