{
  HandlerMethod handlerMethod=this.mappingLookup.get(mapping);
  if (handlerMethod != null && !handlerMethod.equals(newHandlerMethod)) {
    throw new IllegalStateException("Ambiguous mapping. Cannot map '" + newHandlerMethod.getBean() + "' method \n"+ newHandlerMethod+ "\nto "+ mapping+ ": There is already '"+ handlerMethod.getBean()+ "' bean method\n"+ handlerMethod+ " mapped.");
  }
  MappingDefinition<T> definition=this.methodLookup.get(newHandlerMethod.getMethod());
  if (definition != null) {
    throw new IllegalStateException("Cannot map " + newHandlerMethod.getMethod() + "\nto "+ mapping+ ".\n It is already mapped to "+ definition.getMapping());
  }
}
