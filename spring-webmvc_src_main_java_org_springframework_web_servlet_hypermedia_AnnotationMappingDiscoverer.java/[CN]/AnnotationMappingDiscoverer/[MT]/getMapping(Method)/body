{
  String[] mapping=getMappingFrom(attribute.findValueOn(method));
  if (mapping.length > 1) {
    throw new IllegalStateException(String.format("Multiple method level mappings defined on method %s!",method.toString()));
  }
  String typeMapping=getMapping(method.getDeclaringClass());
  if (mapping == null || mapping.length == 0) {
    return typeMapping;
  }
  return typeMapping == null || "/".equals(typeMapping) ? mapping[0] : typeMapping + mapping[0];
}
