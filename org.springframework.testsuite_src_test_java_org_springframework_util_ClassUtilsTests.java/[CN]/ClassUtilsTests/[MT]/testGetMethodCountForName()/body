{
  assertEquals("Verifying number of overloaded 'print' methods for OverloadedMethodsClass.",2,ClassUtils.getMethodCountForName(OverloadedMethodsClass.class,"print"));
  assertEquals("Verifying number of overloaded 'print' methods for SubOverloadedMethodsClass.",4,ClassUtils.getMethodCountForName(SubOverloadedMethodsClass.class,"print"));
}
