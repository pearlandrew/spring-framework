{
  StringBuffer sb=new StringBuffer("ReflectiveMethodInvocation: ");
  sb.append(this.method).append("; ");
  if (this.target == null) {
    sb.append("target is null");
  }
 else {
    sb.append("target is of class [").append(this.target.getClass().getName()).append(']');
  }
  return sb.toString();
}
