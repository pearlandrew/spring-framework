{
  if (source == null) {
    assertNotPrimitiveTargetType(sourceType,targetType);
    return source;
  }
 else   if (sourceType.isAssignableTo(targetType) && targetType.getObjectType().isInstance(source)) {
    return source;
  }
 else {
    throw new ConverterNotFoundException(sourceType,targetType);
  }
}
