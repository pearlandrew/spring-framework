{
  ConvertersForPair convertersForPair=converters.get(new ConvertiblePair(sourceCandidate.getType(),targetCandidate.getType()));
  GenericConverter converter=convertersForPair == null ? null : convertersForPair.getConverter(sourceType,targetType);
  if (converter != null) {
    return converter;
  }
  return null;
}
