{
  this.mockServer.expect(requestTo("/composers")).andExpect(content().mimeType("application/json;charset=UTF-8")).andExpect(jsonPath("$.composers[0].name").value("Johann Sebastian Bach")).andExpect(jsonPath("$.performers[1].name").value("Yehudi Menuhin")).andExpect(jsonPath("$.composers[0].name").value(equalTo("Johann Sebastian Bach"))).andExpect(jsonPath("$.performers[1].name").value(equalTo("Yehudi Menuhin"))).andRespond(withSuccess());
  this.restTemplate.put(new URI("/composers"),this.people);
  this.mockServer.verify();
}
