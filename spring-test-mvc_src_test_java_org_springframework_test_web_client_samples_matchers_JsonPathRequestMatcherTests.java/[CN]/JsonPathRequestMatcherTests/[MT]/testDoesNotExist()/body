{
  this.mockServer.expect(requestTo("/composers")).andExpect(content().contentType("application/json;charset=UTF-8")).andExpect(jsonPath("$.composers[?(@.name == 'Edvard Grieeeeeeg')]").doesNotExist()).andExpect(jsonPath("$.composers[?(@.name == 'Robert Schuuuuuuman')]").doesNotExist()).andExpect(jsonPath("$.composers[-1]").doesNotExist()).andExpect(jsonPath("$.composers[4]").doesNotExist()).andRespond(withSuccess());
  this.restTemplate.put(new URI("/composers"),this.people);
  this.mockServer.verify();
}
