{
  try {
    this.template=getTemplate();
  }
 catch (  ResourceNotFoundException ex) {
    throw new ApplicationContextException("Cannot find Velocity template for URL [" + getUrl() + "]: Did you specify the correct resource loader path?",ex);
  }
catch (  Exception ex) {
    throw new ApplicationContextException("Could not load Velocity template for URL [" + getUrl() + "]",ex);
  }
}
