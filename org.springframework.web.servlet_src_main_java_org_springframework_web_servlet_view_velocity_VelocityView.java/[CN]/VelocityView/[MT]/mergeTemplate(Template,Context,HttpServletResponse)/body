{
  try {
    template.merge(context,response.getWriter());
  }
 catch (  MethodInvocationException ex) {
    throw new NestedServletException("Method invocation failed during rendering of Velocity view with name '" + getBeanName() + "': "+ ex.getMessage()+ "; reference ["+ ex.getReferenceName()+ "], method '"+ ex.getMethodName()+ "'",ex.getWrappedThrowable());
  }
}
