{
  this.mockMvc.perform(get("/handle").accept(MediaType.TEXT_PLAIN)).andExpect(content().contentType(MediaType.TEXT_PLAIN)).andExpect(content().contentType("text/plain"));
  this.mockMvc.perform(get("/handleUtf8")).andExpect(content().contentType(MediaType.valueOf("text/plain;charset=UTF-8"))).andExpect(content().contentType("text/plain;charset=UTF-8")).andExpect(content().contentTypeCompatibleWith("text/plan")).andExpect(content().contentTypeCompatibleWith(MediaType.TEXT_PLAIN));
}
