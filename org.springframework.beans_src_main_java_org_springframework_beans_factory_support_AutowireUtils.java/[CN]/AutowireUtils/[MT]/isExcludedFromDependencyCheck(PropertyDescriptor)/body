{
  Method wm=pd.getWriteMethod();
  if (wm == null) {
    return false;
  }
  if (wm.getDeclaringClass().getName().indexOf("$$") == -1) {
    return false;
  }
  Class superclass=wm.getDeclaringClass().getSuperclass();
  return !ClassUtils.hasMethod(superclass,wm.getName(),wm.getParameterTypes());
}
