{
  Method setter=GenericBean.class.getMethod("setResourceList",List.class);
  Assert.assertEquals(Resource.class,GenericCollectionTypeResolver.getCollectionParameterType(new MethodParameter(setter,0)));
  Method getter=GenericBean.class.getMethod("getResourceList");
  Assert.assertEquals(Resource.class,GenericCollectionTypeResolver.getCollectionReturnType(getter));
}
