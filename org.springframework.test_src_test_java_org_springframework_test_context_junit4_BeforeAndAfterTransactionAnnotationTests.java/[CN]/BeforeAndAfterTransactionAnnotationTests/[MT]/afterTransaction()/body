{
  assertInTransaction(false);
  this.inTransaction=false;
  BeforeAndAfterTransactionAnnotationTests.numAfterTransactionCalls++;
  assertEquals("Deleting yoda",1,deletePerson(simpleJdbcTemplate,YODA));
  assertEquals("Verifying the number of rows in the person table after a transactional test method.",0,countRowsInPersonTable(simpleJdbcTemplate));
}
