{
  Mock mock=new Mock();
  List<ConcretePerson> result=mock.getJdbcTemplate().query("select name, age, birth_date, balance from people",new BeanPropertyRowMapper<ConcretePerson>(ConcretePerson.class,true));
  assertEquals(1,result.size());
  verifyConcretePerson(result.get(0));
  mock.verifyClosed();
}
