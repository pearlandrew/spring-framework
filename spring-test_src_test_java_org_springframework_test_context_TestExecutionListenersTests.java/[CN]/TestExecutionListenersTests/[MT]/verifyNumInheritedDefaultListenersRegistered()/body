{
  TestContextManager testContextManager=new TestContextManager(InheritedDefaultListenersExampleTestCase.class);
  assertEquals("Num registered TELs for InheritedDefaultListenersExampleTestCase.",1,testContextManager.getTestExecutionListeners().size());
  testContextManager=new TestContextManager(SubInheritedDefaultListenersExampleTestCase.class);
  assertEquals("Num registered TELs for SubInheritedDefaultListenersExampleTestCase.",1,testContextManager.getTestExecutionListeners().size());
  testContextManager=new TestContextManager(SubSubInheritedDefaultListenersExampleTestCase.class);
  assertEquals("Num registered TELs for SubSubInheritedDefaultListenersExampleTestCase.",2,testContextManager.getTestExecutionListeners().size());
}
