{
  try {
    Object value=getModel().getValueType().newInstance();
    getModel().setValue(value);
  }
 catch (  InstantiationException e) {
    throw new IllegalStateException("Could not lazily instantiate object of type [" + getModel().getValueType().getName() + "] to access property"+ property,e);
  }
catch (  IllegalAccessException e) {
    throw new IllegalStateException("Could not lazily instantiate object of type [" + getModel().getValueType().getName() + "] to access property"+ property,e);
  }
}
