{
  if (!isDirty()) {
    throw new IllegalStateException("Binding not dirty; nothing to commit");
  }
  if (!isValid()) {
    throw new IllegalStateException("Binding is invalid; only commit valid bindings");
  }
  try {
    getModel().setValue(bufferedValue);
    this.bufferedValue=null;
  }
 catch (  Exception e) {
  }
}
