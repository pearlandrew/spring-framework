{
  RequestPart annot=parameter.getParameterAnnotation(RequestPart.class);
  String partName=annot.value();
  if (partName.length() == 0) {
    partName=parameter.getParameterName();
    Assert.notNull(partName,"Request part name for argument type [" + parameter.getParameterType().getName() + "] not available, and parameter name information not found in class file either.");
  }
  return partName;
}
