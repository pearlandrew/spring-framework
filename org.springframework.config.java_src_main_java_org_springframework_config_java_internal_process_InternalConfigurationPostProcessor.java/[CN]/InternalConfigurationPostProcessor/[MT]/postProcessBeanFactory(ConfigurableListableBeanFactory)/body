{
  if (!(beanFactory instanceof DefaultListableBeanFactory))   throw new IllegalStateException("beanFactory must be of type " + DefaultListableBeanFactory.class.getSimpleName());
  ConfigurationModel model=new ConfigurationModel();
  parseAnyConfigurationClasses(beanFactory,model);
  enhanceAnyConfigurationClasses((DefaultListableBeanFactory)beanFactory,model);
}
