{
  pmf.getPersistenceManager();
  pmfControl.setReturnValue(pm);
  pm.makePersistent("0x");
  pmControl.setReturnValue("0",1);
  pm.close();
  pmControl.setVoidCallable(1);
  pmfControl.replay();
  pmControl.replay();
  JdoTemplate jt=new JdoTemplate(pmf);
  assertEquals("0",jt.attachCopy("0x"));
}
