{
  if (message == null) {
    return null;
  }
  try {
    return (Message<?>)this.jmsMessageConverter.fromMessage(message);
  }
 catch (  JMSException ex) {
    throw new MessageConversionException("Could not convert '" + message + "'",ex);
  }
catch (  JmsException ex) {
    throw new MessageConversionException("Could not convert '" + message + "'",ex);
  }
}
