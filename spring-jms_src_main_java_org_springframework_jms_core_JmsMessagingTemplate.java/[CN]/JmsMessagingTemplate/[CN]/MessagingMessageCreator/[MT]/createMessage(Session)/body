{
  try {
    return this.messageConverter.toMessage(this.message,session);
  }
 catch (  JMSException ex) {
    throw new MessageConversionException("Could not convert '" + message + "'",ex);
  }
catch (  JmsException ex) {
    throw new MessageConversionException("Could not convert '" + message + "'",ex);
  }
}
