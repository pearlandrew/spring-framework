{
  Object key="enescu";
  Object value1="george";
  Object value2="geo";
  assertNull(cache.get("enescu"));
  cache.put(key,value1);
  assertEquals(value1,cache.get(key));
  cache.replace(key,value2,value1);
  assertEquals(value1,cache.get(key));
  cache.replace(key,value1,value2);
  assertEquals(value2,cache.get(key));
  cache.replace(key,value2,value1);
  assertEquals(value1,cache.get(key));
}
