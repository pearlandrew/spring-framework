{
  if (this.expressionPattern.matches(advisedBeanName)) {
    return true;
  }
  if (beanFactory != null) {
    String[] aliases=beanFactory.getAliases(advisedBeanName);
    for (int i=0; i < aliases.length; i++) {
      if (this.expressionPattern.matches(aliases[i])) {
        return true;
      }
    }
  }
  return false;
}
