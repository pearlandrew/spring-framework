{
  Assert.state(this.cacheResolver != null,"'cacheResolver' is required. Either set the cache resolver " + "to use or set the cache manager to create a default cache resolver based on it.");
  Assert.state(this.cacheOperationSource != null,"The 'cacheOperationSources' property is required: " + "If there are no cacheable methods, then don't use a cache aspect.");
  Assert.state(this.applicationContext != null,"The application context was not injected as it should.");
  this.initialized=true;
}
