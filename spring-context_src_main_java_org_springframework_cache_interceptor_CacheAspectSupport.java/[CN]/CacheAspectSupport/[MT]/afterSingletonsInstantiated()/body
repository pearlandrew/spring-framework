{
  if (getCacheResolver() == null) {
    CacheManager cacheManager=this.applicationContext.getBean(CacheManager.class);
    if (cacheManager == null) {
      throw new IllegalStateException("No bean of type CacheManager could be found. " + "Register a CacheManager bean or remove the @EnableCaching annotation " + "from your configuration.");
    }
    setCacheManager(cacheManager);
  }
  Assert.state(this.cacheResolver != null,"'cacheResolver' is required. Either set the cache resolver " + "to use or set the cache manager to create a default cache resolver based on it.");
  this.initialized=true;
}
