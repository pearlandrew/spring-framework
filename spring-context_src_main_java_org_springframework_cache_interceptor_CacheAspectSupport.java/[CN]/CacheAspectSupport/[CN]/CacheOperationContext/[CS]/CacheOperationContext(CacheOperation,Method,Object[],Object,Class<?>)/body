{
  this.operation=operation;
  this.method=method;
  this.args=extractArgs(method,args);
  this.target=target;
  this.targetClass=targetClass;
  this.caches=CacheAspectSupport.this.getCaches(operation);
  if (StringUtils.hasText(operation.getKeyGenerator())) {
    this.operationKeyGenerator=BeanFactoryAnnotationUtils.qualifiedBeanOfType(applicationContext,KeyGenerator.class,operation.getKeyGenerator());
  }
 else {
    this.operationKeyGenerator=keyGenerator;
  }
}
