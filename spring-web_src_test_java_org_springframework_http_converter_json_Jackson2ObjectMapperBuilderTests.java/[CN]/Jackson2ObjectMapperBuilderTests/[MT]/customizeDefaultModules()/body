{
  ObjectMapper objectMapper=Jackson2ObjectMapperBuilder.json().featuresToDisable(SerializationFeature.WRITE_DATES_AS_TIMESTAMPS).modulesToInstall(CustomModule.class).build();
  DateTime dateTime=DateTime.parse("2011-12-03T10:15:30");
  assertEquals("\"2011-12-03\"",new String(objectMapper.writeValueAsBytes(dateTime),"UTF-8"));
}
