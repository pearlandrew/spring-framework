{
  if (CollectionUtils.isEmpty(classes)) {
    return "[]";
  }
  StringBuffer sb=new StringBuffer("[");
  for (Iterator it=classes.iterator(); it.hasNext(); ) {
    Class clazz=(Class)it.next();
    sb.append(clazz.getName());
    if (it.hasNext()) {
      sb.append(", ");
    }
  }
  sb.append("]");
  return sb.toString();
}
