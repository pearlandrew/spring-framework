{
  Method result=method;
  if (method != null && !Modifier.isPrivate(method.getModifiers()) && targetClass != null && !targetClass.equals(method.getDeclaringClass())) {
    try {
      result=targetClass.getDeclaredMethod(method.getName(),method.getParameterTypes());
    }
 catch (    NoSuchMethodException ex) {
    }
  }
  return result;
}
