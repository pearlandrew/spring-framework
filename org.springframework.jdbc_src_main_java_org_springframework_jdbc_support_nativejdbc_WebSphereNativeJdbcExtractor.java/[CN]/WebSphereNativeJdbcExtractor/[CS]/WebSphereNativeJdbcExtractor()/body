{
  try {
    this.webSphere5ConnectionClass=getClass().getClassLoader().loadClass(JDBC_ADAPTER_CONNECTION_NAME_5);
    Class jdbcAdapterUtilClass=getClass().getClassLoader().loadClass(JDBC_ADAPTER_UTIL_NAME_5);
    this.webSphere5NativeConnectionMethod=jdbcAdapterUtilClass.getMethod("getNativeConnection",new Class[]{this.webSphere5ConnectionClass});
  }
 catch (  Exception ex) {
    throw new IllegalStateException("Could not initialize WebSphereNativeJdbcExtractor because WebSphere API classes are not available: " + ex);
  }
}
