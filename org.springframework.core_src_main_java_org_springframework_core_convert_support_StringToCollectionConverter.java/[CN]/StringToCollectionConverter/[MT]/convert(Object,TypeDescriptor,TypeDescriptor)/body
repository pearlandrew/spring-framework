{
  if (source == null) {
    return null;
  }
  String string=(String)source;
  String[] fields=StringUtils.commaDelimitedListToStringArray(string);
  Collection target=CollectionFactory.createCollection(targetType.getType(),fields.length);
  for (  String sourceElement : fields) {
    Object targetElement=this.conversionService.convert(sourceElement.trim(),sourceType,targetType.getElementTypeDescriptor(sourceElement));
    target.add(targetElement);
  }
  return target;
}
