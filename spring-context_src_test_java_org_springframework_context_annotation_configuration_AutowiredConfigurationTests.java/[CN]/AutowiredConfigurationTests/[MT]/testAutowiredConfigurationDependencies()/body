{
  ClassPathXmlApplicationContext factory=new ClassPathXmlApplicationContext(AutowiredConfigurationTests.class.getSimpleName() + ".xml",AutowiredConfigurationTests.class);
  assertThat(factory.getBean("colour",Colour.class),equalTo(Colour.RED));
  assertThat(factory.getBean("testBean",TestBean.class).getName(),equalTo(Colour.RED.toString()));
}
