{
  if (Modifier.isPrivate(getModifiers()))   problemReporter.error(new PrivateMethodError());
  if (Modifier.isFinal(getModifiers()))   problemReporter.error(new FinalMethodError());
  Scope scope=this.getAnnotation(Scope.class);
  if (scope != null && scope.proxyMode() != ScopedProxyMode.NO && (scope.value().equals(SINGLETON) || scope.value().equals(PROTOTYPE)))   problemReporter.error(new InvalidScopedProxyDeclarationError(this));
}
