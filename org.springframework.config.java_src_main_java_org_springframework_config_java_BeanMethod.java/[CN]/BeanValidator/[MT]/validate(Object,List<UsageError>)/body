{
  BeanMethod method=(BeanMethod)object;
  if (method.getAnnotation(ScopedProxy.class) == null)   return;
  Bean bean=method.getRequiredAnnotation(Bean.class);
  if (bean.scope().equals(StandardScopes.SINGLETON) || bean.scope().equals(StandardScopes.PROTOTYPE))   errors.add(new InvalidScopedProxyDeclarationError(method));
}
