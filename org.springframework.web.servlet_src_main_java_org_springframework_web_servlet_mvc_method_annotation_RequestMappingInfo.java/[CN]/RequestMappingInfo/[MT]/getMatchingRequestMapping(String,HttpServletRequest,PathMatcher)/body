{
  if (!checkMethod(request) || !paramsCondition.match(request) || !headersCondition.match(request)|| !consumesCondition.match(request)) {
    return null;
  }
 else {
    List<String> matchingPatterns=getMatchingPatterns(lookupPath,request,pathMatcher);
    if (!matchingPatterns.isEmpty()) {
      Set<RequestMethod> matchingMethods=getMatchingMethod(request);
      return new RequestMappingInfo(matchingPatterns,matchingMethods,this.paramsCondition,this.headersCondition,this.consumesCondition);
    }
 else {
      return null;
    }
  }
}
