{
  if (!this.running) {
    if (logger.isTraceEnabled()) {
      logger.trace("STOMP broker relay not running. Ignoring message id=" + message.getHeaders().getId());
    }
    return;
  }
  if (logger.isTraceEnabled()) {
    logger.trace("Handling message " + message);
  }
  handleMessageInternal(message);
}
