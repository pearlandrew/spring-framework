{
  if (obj == this) {
    return true;
  }
  if (obj instanceof EncodedResource) {
    EncodedResource that=(EncodedResource)obj;
    return (this.resource.equals(that.resource) && ObjectUtils.nullSafeEquals(this.charset,that.charset) && ObjectUtils.nullSafeEquals(this.encoding,that.encoding));
  }
  return false;
}
