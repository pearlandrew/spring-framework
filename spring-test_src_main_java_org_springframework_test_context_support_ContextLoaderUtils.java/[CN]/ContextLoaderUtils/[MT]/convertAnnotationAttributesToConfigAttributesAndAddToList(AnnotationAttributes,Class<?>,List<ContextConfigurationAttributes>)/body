{
  if (logger.isTraceEnabled()) {
    logger.trace(String.format("Retrieved @ContextConfiguration attributes [%s] for declaring class [%s].",annAttrs,declaringClass.getName()));
  }
  ContextConfigurationAttributes attributes=new ContextConfigurationAttributes(declaringClass,annAttrs);
  if (logger.isTraceEnabled()) {
    logger.trace("Resolved context configuration attributes: " + attributes);
  }
  attributesList.add(attributes);
}
