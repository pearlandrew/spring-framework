{
  this.mockMvc.perform(get("/music/people")).andExpect(xpath("/ns:people/composers/composer",NS).nodeCount(4)).andExpect(xpath("/ns:people/performers/performer",NS).nodeCount(2)).andExpect(xpath("/ns:people/composers/composer",NS).nodeCount(equalTo(4))).andExpect(xpath("/ns:people/performers/performer",NS).nodeCount(equalTo(2)));
}
