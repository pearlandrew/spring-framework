{
  String s=Long.toString(getNextKey());
  int len=s.length();
  if (len < this.paddingLength) {
    StringBuffer buf=new StringBuffer(this.paddingLength);
    for (int i=0; i < this.paddingLength - len; i++) {
      buf.append('0');
    }
    buf.append(s);
    s=buf.toString();
  }
  return s;
}
