{
  assertCondition(mav != null,"ModelAndView is null");
  List modelList=assertAndReturnModelAttributeOfType(mav,modelName,List.class);
  assertCondition(expectedList.size() == modelList.size(),"Size of model list is '" + modelList.size() + "' while size of expected list is '"+ expectedList.size()+ "'");
  if (comparator != null) {
    Collections.sort(modelList,comparator);
    Collections.sort(expectedList,comparator);
  }
 else {
    Collections.sort(modelList);
    Collections.sort(expectedList);
  }
  assertCondition(expectedList.equals(modelList),"List in model under name '" + modelName + "' is not equal to the expected list.");
}
