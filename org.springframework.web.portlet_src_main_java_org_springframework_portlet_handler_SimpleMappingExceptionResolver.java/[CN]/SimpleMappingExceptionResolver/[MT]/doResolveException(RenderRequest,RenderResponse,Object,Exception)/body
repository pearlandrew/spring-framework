{
  if (logger.isDebugEnabled()) {
    logger.debug("Resolving exception from handler [" + handler + "]: "+ ex);
  }
  logException(ex,request);
  String viewName=determineViewName(ex,request);
  if (viewName != null) {
    return getModelAndView(viewName,ex,request);
  }
 else {
    return null;
  }
}
