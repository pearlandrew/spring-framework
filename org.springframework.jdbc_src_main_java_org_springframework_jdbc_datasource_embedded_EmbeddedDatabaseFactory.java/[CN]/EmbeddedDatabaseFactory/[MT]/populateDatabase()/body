{
  try {
    Connection connection=this.dataSource.getConnection();
    try {
      this.databasePopulator.populate(connection);
    }
  finally {
      try {
        connection.close();
      }
 catch (      SQLException ex) {
      }
    }
  }
 catch (  Exception ex) {
    throw new DataAccessResourceFailureException("Failed to populate database",ex);
  }
}
