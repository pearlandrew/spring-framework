{
  List<HttpMethod> result=new ArrayList<HttpMethod>();
  String value=getFirst(ACCESS_CONTROL_ALLOW_METHODS);
  if (value != null) {
    String[] tokens=value.split(",\\s*");
    for (    String token : tokens) {
      HttpMethod resolved=HttpMethod.resolve(token);
      if (resolved != null) {
        result.add(resolved);
      }
    }
  }
  return result;
}
