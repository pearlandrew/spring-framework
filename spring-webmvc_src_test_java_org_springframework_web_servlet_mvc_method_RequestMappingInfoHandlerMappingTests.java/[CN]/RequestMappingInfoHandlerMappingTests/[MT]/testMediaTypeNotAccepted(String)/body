{
  try {
    MockHttpServletRequest request=new MockHttpServletRequest("GET",url);
    request.addHeader("Accept","application/json");
    this.mapping.getHandler(request);
    fail("HttpMediaTypeNotAcceptableException expected");
  }
 catch (  HttpMediaTypeNotAcceptableException ex) {
    assertEquals("Invalid supported producible media types",Arrays.asList(new MediaType("application","xml")),ex.getSupportedMediaTypes());
  }
}
