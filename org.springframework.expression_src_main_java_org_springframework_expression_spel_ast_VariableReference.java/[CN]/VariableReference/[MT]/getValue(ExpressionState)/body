{
  if (name.equals(THIS))   return state.getActiveContextObject();
  if (name.equals(ROOT))   return state.getRootContextObject();
  Object result=state.lookupVariable(name);
  if (result == null) {
    throw new SpelException(getCharPositionInLine(),SpelMessages.VARIABLE_NOT_FOUND,name);
  }
  return result;
}
