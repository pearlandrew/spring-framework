{
  Flux<DataBuffer> source=Flux.just(stringBuffer(POJO_CHILD));
  Flux<Object> output=this.decoder.decode(source,ResolvableType.forClass(TypePojo.class),null);
  TestSubscriber<Object> testSubscriber=new TestSubscriber<>();
  testSubscriber.bindTo(output).assertNoError().assertComplete().assertValues(new TypePojo("foo","bar"),new TypePojo("foofoo","barbar"));
}
