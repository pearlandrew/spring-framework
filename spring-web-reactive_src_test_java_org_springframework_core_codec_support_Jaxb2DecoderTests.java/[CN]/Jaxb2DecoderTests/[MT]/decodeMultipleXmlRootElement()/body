{
  Flux<DataBuffer> source=Flux.just(stringBuffer(POJO_CHILD));
  Flux<Object> output=this.decoder.decode(source,ResolvableType.forClass(Pojo.class),null);
  TestSubscriber<Object> testSubscriber=new TestSubscriber<>();
  testSubscriber.bindTo(output).assertNoError().assertComplete().assertValues(new Pojo("foo","bar"),new Pojo("foofoo","barbar"));
}
