{
  Flux<DataBuffer> source=Flux.just(stringBuffer(POJO_ROOT));
  Flux<Object> output=decoder.decode(source,ResolvableType.forClass(TypePojo.class),null);
  TestSubscriber<Object> testSubscriber=new TestSubscriber<>();
  testSubscriber.bindTo(output).assertNoError().assertComplete().assertValues(new TypePojo("foofoo","barbar"));
}
