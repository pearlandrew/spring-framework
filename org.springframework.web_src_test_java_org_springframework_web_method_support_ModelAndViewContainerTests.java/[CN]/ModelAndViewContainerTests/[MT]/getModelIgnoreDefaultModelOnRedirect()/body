{
  this.mavContainer.addAttribute("name","value");
  this.mavContainer.setRedirectModelScenario(true);
  assertEquals("Default model should be used since no redirect model was provided",1,this.mavContainer.getModel().size());
  this.mavContainer.setIgnoreDefaultModelOnRedirect(true);
  assertEquals("Empty model should be returned if no redirect model is available",0,this.mavContainer.getModel().size());
}
