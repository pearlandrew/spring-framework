{
  ServletOutputStream output=outputStream();
  if (output.isReady()) {
    if (logger.isTraceEnabled()) {
      this.logger.trace("flush");
    }
    try {
      output.flush();
      this.flushOnNext=false;
    }
 catch (    IOException ignored) {
    }
  }
 else {
    this.flushOnNext=true;
  }
}
