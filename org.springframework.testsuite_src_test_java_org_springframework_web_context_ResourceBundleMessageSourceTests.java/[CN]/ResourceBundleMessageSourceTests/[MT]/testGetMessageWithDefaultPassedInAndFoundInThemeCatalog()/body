{
  String msg=getThemeMessage("theme.example1",null,"This is a default theme msg if not found in theme cat.",Locale.US);
  assertTrue("valid msg from theme resourcebundle with default msg passed in returned default msg.  Expected msg from catalog. Received: " + msg,msg.equals("This is a test message in the theme message catalog."));
  msg=getThemeMessage("theme.example1",null,"This is a default theme msg if not found in theme cat.",Locale.UK);
  assertTrue("valid msg from theme resourcebundle with default msg passed in returned default msg.  Expected msg from catalog.",msg.equals("This is a test message in the theme message catalog with no args."));
}
