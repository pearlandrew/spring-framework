{
  StringBuffer sb=new StringBuffer();
  if (getChildCount() == 1) {
    sb.append(getChild(0).toStringAST());
  }
 else {
    sb.append("(");
    for (int i=0; i < getChildCount(); i++) {
      if (i > 0)       sb.append(";");
      sb.append(getChild(i).toStringAST());
    }
    sb.append(")");
  }
  return sb.toString();
}
