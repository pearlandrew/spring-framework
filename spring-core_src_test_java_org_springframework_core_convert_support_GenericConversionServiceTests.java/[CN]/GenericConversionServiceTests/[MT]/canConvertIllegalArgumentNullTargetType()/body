{
  try {
    assertFalse(conversionService.canConvert(String.class,null));
    fail("Should have failed");
  }
 catch (  IllegalArgumentException ex) {
  }
  try {
    assertFalse(conversionService.canConvert(TypeDescriptor.valueOf(String.class),null));
    fail("Should have failed");
  }
 catch (  IllegalArgumentException ex) {
  }
}
