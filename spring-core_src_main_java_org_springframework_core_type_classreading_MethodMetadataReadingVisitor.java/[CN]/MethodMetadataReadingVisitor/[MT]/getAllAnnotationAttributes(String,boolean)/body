{
  if (!this.attributeMap.containsKey(annotationType)) {
    return null;
  }
  MultiValueMap<String,Object> allAttributes=new LinkedMultiValueMap<String,Object>();
  for (  AnnotationAttributes annotationAttributes : this.attributeMap.get(annotationType)) {
    for (    Map.Entry<String,Object> entry : AnnotationReadingVisitorUtils.convertClassValues(this.classLoader,annotationAttributes,classValuesAsString).entrySet()) {
      allAttributes.add(entry.getKey(),entry.getValue());
    }
  }
  return allAttributes;
}
