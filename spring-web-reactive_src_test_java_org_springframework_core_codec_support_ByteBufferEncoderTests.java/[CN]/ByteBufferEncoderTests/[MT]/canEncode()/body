{
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(ByteBuffer.class),MediaType.TEXT_PLAIN));
  assertFalse(this.encoder.canEncode(ResolvableType.forClass(Integer.class),MediaType.TEXT_PLAIN));
  assertTrue(this.encoder.canEncode(ResolvableType.forClass(ByteBuffer.class),MediaType.APPLICATION_JSON));
}
