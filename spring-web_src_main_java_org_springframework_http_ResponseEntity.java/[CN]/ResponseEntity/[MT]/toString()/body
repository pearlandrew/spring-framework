{
  StringBuilder builder=new StringBuilder("<");
  builder.append(statusCode.toString());
  builder.append(' ');
  builder.append(statusCode.getReasonPhrase());
  builder.append(',');
  T body=getBody();
  HttpHeaders headers=getHeaders();
  if (body != null) {
    builder.append(body);
    if (headers != null) {
      builder.append(',');
    }
  }
  if (headers != null) {
    builder.append(headers);
  }
  builder.append('>');
  return builder.toString();
}
