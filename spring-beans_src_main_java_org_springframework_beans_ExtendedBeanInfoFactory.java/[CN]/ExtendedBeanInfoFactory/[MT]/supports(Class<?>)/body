{
  for (  Method method : beanClass.getMethods()) {
    String methodName=method.getName();
    Class<?>[] parameterTypes=method.getParameterTypes();
    if (Modifier.isPublic(method.getModifiers()) && methodName.length() > 3 && methodName.startsWith("set") && (parameterTypes.length == 1 || (parameterTypes.length == 2 && parameterTypes[0].equals(int.class))) && !void.class.isAssignableFrom(method.getReturnType())) {
      return true;
    }
  }
  return false;
}
