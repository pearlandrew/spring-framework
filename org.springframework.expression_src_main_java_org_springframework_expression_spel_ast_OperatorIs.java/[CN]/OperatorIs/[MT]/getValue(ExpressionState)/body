{
  Object left=getLeftOperand().getValue(state);
  Object right=getRightOperand().getValue(state);
  if (!(right instanceof Class<?>)) {
    throw new SpelException(getRightOperand().getCharPositionInLine(),SpelMessages.IS_OPERATOR_NEEDS_CLASS_OPERAND,right.getClass().getName());
  }
  return (((Class<?>)right).isAssignableFrom(left.getClass()));
}
