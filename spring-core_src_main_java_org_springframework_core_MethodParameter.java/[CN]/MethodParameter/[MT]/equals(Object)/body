{
  if (this == obj) {
    return true;
  }
  if (obj != null && obj instanceof MethodParameter) {
    MethodParameter other=(MethodParameter)obj;
    if (this.parameterIndex != other.parameterIndex) {
      return false;
    }
 else     if (this.getMember().equals(other.getMember())) {
      return true;
    }
 else {
      return false;
    }
  }
  return false;
}
