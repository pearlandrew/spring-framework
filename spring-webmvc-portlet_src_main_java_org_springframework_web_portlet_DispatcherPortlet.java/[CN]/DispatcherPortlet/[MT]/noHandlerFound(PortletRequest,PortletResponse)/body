{
  if (pageNotFoundLogger.isWarnEnabled()) {
    pageNotFoundLogger.warn("No handler found for current request " + "in DispatcherPortlet with name '" + getPortletName() + "', mode '"+ request.getPortletMode()+ "', phase '"+ request.getAttribute(PortletRequest.LIFECYCLE_PHASE)+ "', parameters "+ StylerUtils.style(request.getParameterMap()));
  }
  throw new NoHandlerFoundException("No handler found for portlet request",request);
}
