{
  for (  ViewResolutionRegistration<?> existingRegistration : this.registrations) {
    if (existingRegistration.getClass().equals(registration.getClass())) {
      throw new IllegalStateException("An instance of " + registration.getClass().getSimpleName() + " is already registered, and multiple view resolvers and configurers beans are not supported by this registry");
    }
  }
  registrations.add(registration);
}
