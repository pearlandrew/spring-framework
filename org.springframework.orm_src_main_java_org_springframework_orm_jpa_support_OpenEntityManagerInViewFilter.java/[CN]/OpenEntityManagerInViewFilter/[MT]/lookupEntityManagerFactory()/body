{
  if (logger.isDebugEnabled()) {
    logger.debug("Using EntityManagerFactory '" + getEntityManagerFactoryBeanName() + "' for OpenEntityManagerInViewFilter");
  }
  WebApplicationContext wac=WebApplicationContextUtils.getRequiredWebApplicationContext(getServletContext());
  return (EntityManagerFactory)wac.getBean(getEntityManagerFactoryBeanName(),EntityManagerFactory.class);
}
