{
  if (String.class.equals(targetType.getType())) {
    return this.conversionService.canConvert(sourceType.getMapKeyTypeDescriptor(),targetType.getElementTypeDescriptor()) && this.conversionService.canConvert(sourceType.getMapValueTypeDescriptor(),targetType.getElementTypeDescriptor());
  }
 else {
    return this.conversionService.canConvert(sourceType.getMapValueTypeDescriptor(),targetType.getElementTypeDescriptor());
  }
}
