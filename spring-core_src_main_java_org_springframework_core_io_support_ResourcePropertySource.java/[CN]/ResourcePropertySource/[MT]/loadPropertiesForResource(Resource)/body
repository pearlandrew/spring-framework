{
  Properties props=new Properties();
  InputStream is=resource.getInputStream();
  String filename=resource.getFilename();
  if (filename != null && filename.endsWith(".xml")) {
    props.loadFromXML(is);
  }
 else {
    props.load(is);
  }
  try {
    is.close();
  }
 catch (  IOException ex) {
  }
  return props;
}
