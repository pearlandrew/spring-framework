{
  Assert.notNull(marshaller,"marshaller must not be null");
  if (!(marshaller instanceof Unmarshaller)) {
    throw new IllegalArgumentException("Marshaller [" + marshaller + "] does not implement the Unmarshaller "+ "interface. Please set an Unmarshaller explicitely by using the "+ "MarshallingHttpMessageConverter(Marshaller, Unmarshaller) constructor.");
  }
 else {
    this.marshaller=marshaller;
    this.unmarshaller=(Unmarshaller)marshaller;
  }
}
