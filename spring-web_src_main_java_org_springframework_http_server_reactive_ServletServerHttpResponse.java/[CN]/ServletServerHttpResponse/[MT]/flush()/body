{
  ServletOutputStream outputStream=this.response.getOutputStream();
  if (outputStream.isReady()) {
    try {
      outputStream.flush();
      this.flushOnNext=false;
    }
 catch (    IOException ex) {
      this.flushOnNext=true;
      throw ex;
    }
  }
 else {
    this.flushOnNext=true;
  }
}
