{
  if (firstBytePos < 0) {
    throw new IllegalArgumentException("Invalid firstPos=" + firstBytePos);
  }
  if (lastBytePos != null && lastBytePos < firstBytePos) {
    throw new IllegalArgumentException("firstPost= " + firstBytePos + " should be less then or equal to lastBytePosition="+ lastBytePos);
  }
}
