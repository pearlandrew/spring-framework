{
  return new ToStringCreator(this).append("testClass",testClass).append("locations",ObjectUtils.nullSafeToString(locations)).append("classes",ObjectUtils.nullSafeToString(classes)).append("contextInitializerClasses",ObjectUtils.nullSafeToString(contextInitializerClasses)).append("activeProfiles",ObjectUtils.nullSafeToString(activeProfiles)).append("contextLoader",nullSafeToString(contextLoader)).append("parent",parent).toString();
}
