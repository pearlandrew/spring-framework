{
  List list=(List)binding.getValue();
  if (list == null) {
    list=newListValue(binding.getValueType());
    binding.applySourceValue(list);
    binding.commit();
    list=(List)binding.getValue();
  }
  if (index >= list.size()) {
    for (int i=list.size(); i <= index; i++) {
      list.add(newValue(getElementType()));
    }
  }
}
