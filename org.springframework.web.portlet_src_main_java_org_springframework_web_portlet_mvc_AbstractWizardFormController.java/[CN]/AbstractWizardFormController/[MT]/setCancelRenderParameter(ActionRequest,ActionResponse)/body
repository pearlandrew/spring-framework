{
  if (logger.isDebugEnabled())   logger.debug("Setting cancel render parameter [" + PARAM_CANCEL + "]");
  try {
    String name=PortletUtils.getSubmitParameter(request,PARAM_CANCEL);
    if (name != null)     response.setRenderParameter(name,request.getParameter(name));
  }
 catch (  IllegalStateException ex) {
  }
}
