{
  String[] defNames=getListableBeanFactory().getBeanNamesForType(TestBean.class,true,false);
  assertTrue("We should have " + count + " beans for class org.springframework.beans.TestBean, not "+ defNames.length,defNames.length == count);
  int countIncludingFactoryBeans=count + 2;
  String[] names=getListableBeanFactory().getBeanNamesForType(TestBean.class,true,true);
  assertTrue("We should have " + countIncludingFactoryBeans + " beans for class org.springframework.beans.TestBean, not "+ names.length,names.length == countIncludingFactoryBeans);
}
