{
  if (candidate == null) {
    return false;
  }
  if (candidate == parent) {
    return true;
  }
  ClassLoader classLoaderToCheck=candidate;
  while (classLoaderToCheck != null) {
    classLoaderToCheck=classLoaderToCheck.getParent();
    if (classLoaderToCheck == parent) {
      return true;
    }
  }
  return false;
}
