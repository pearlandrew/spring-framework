{
  Method bridgedMethod=TestEmailProvider.class.getDeclaredMethod("findBy",EmailSearchConditions.class);
  assertNotNull(bridgedMethod);
  assertFalse(bridgedMethod.isBridge());
  Method bridgeMethod=TestEmailProvider.class.getDeclaredMethod("findBy",Object.class);
  assertNotNull(bridgeMethod);
  assertTrue(bridgeMethod.isBridge());
  assertEquals(bridgedMethod,BridgeMethodResolver.findBridgedMethod(bridgeMethod));
}
