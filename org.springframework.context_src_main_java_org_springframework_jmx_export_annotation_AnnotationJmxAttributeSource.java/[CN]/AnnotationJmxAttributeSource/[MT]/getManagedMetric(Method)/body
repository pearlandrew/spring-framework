{
  org.springframework.jmx.export.annotation.ManagedMetric ann=AnnotationUtils.findAnnotation(method,org.springframework.jmx.export.annotation.ManagedMetric.class);
  if (ann == null) {
    return null;
  }
  ManagedMetric managedMetric=new ManagedMetric();
  AnnotationBeanUtils.copyPropertiesToBean(ann,managedMetric);
  return managedMetric;
}
