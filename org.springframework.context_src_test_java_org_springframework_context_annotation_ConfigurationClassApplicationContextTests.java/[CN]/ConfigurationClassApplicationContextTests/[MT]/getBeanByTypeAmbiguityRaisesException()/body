{
  AnnotationConfigApplicationContext context=new AnnotationConfigApplicationContext(TwoTestBeanConfig.class);
  try {
    context.getBean(TestBean.class);
  }
 catch (  RuntimeException ex) {
    assertThat(ex.getMessage(),allOf(containsString("No unique bean of type [" + TestBean.class.getName() + "] is defined"),containsString("tb1"),containsString("tb2")));
  }
}
