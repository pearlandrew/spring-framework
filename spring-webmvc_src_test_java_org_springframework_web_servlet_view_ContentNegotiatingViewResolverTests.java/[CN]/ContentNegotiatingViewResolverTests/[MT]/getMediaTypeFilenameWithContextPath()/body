{
  request.setContextPath("/project-1.0.0.M3");
  request.setRequestURI("/project-1.0.0.M3/");
  assertTrue("Context path should be excluded",viewResolver.getMediaTypes(request).isEmpty());
  request.setRequestURI("/project-1.0.0.M3");
  assertTrue("Context path should be excluded",viewResolver.getMediaTypes(request).isEmpty());
}
