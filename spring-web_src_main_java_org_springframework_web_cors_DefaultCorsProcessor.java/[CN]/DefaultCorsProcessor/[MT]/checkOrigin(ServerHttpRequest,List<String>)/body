{
  String originHeader=request.getHeaders().getOrigin();
  if (originHeader == null || allowedOrigins == null) {
    return false;
  }
  if (allowedOrigins.contains("*")) {
    return true;
  }
  for (  String allowedOrigin : allowedOrigins) {
    if (originHeader.equalsIgnoreCase(allowedOrigin)) {
      return true;
    }
  }
  return false;
}
