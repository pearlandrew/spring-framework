{
  String origin=request.getHeader(HttpHeaders.ORIGIN);
  if ((origin == null) || (allowedOrigins == null)) {
    return false;
  }
  if (allowedOrigins.contains("*")) {
    return true;
  }
  for (  String allowedOrigin : allowedOrigins) {
    if (origin.equalsIgnoreCase(allowedOrigin)) {
      return true;
    }
  }
  return false;
}
