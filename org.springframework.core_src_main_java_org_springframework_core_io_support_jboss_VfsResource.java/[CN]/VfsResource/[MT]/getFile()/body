{
  if (VFSUtils.isNestedFile(file)) {
    throw new IOException("This resource is a nested resource: " + file);
  }
  try {
    return new File(VFSUtils.getCompatibleURI(file));
  }
 catch (  IOException e) {
    throw e;
  }
catch (  Exception e) {
    IOException ioe=new IOException(e.getMessage());
    ioe.initCause(e);
    throw ioe;
  }
}
