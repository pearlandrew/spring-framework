{
  String attributeName=attributeMethod.getName();
  Object value=this.valueCache.get(attributeName);
  if (value == null) {
    value=this.attributeExtractor.getAttributeValue(attributeMethod);
    if (value == null) {
      String msg=String.format("%s returned null for attribute name [%s] from attribute source [%s]",this.attributeExtractor.getClass().getName(),attributeName,this.attributeExtractor.getSource());
      throw new IllegalStateException(msg);
    }
    if (value instanceof Annotation) {
      value=synthesizeAnnotation((Annotation)value,this.attributeExtractor.getAnnotatedElement());
    }
 else     if (value instanceof Annotation[]) {
      value=synthesizeAnnotationArray((Annotation[])value,this.attributeExtractor.getAnnotatedElement());
    }
    this.valueCache.put(attributeName,value);
  }
  if (value.getClass().isArray()) {
    value=cloneArray(value);
  }
  return value;
}
