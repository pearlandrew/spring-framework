{
  InputStream body=this.response.getBody();
  if (body == null) {
    return true;
  }
 else   if (body.markSupported()) {
    body.mark(1);
    if (body.read() == -1) {
      return true;
    }
 else {
      body.reset();
      return false;
    }
  }
 else {
    this.pushbackInputStream=new PushbackInputStream(body);
    int b=pushbackInputStream.read();
    if (b == -1) {
      return true;
    }
 else {
      pushbackInputStream.unread(b);
      return false;
    }
  }
}
