{
  int end=urlPath.indexOf('?');
  if (end == -1) {
    end=urlPath.indexOf('#');
    if (end == -1) {
      end=urlPath.length();
    }
  }
  int begin=urlPath.lastIndexOf('/',end) + 1;
  int paramIndex=urlPath.indexOf(';',begin);
  end=(paramIndex != -1 && paramIndex < end ? paramIndex : end);
  return urlPath.substring(begin,end);
}
