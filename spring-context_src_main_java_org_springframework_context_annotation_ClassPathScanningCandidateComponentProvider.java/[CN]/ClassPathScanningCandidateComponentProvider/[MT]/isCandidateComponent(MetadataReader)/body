{
  for (  TypeFilter tf : this.excludeFilters) {
    if (tf.match(metadataReader,this.metadataReaderFactory)) {
      return false;
    }
  }
  for (  TypeFilter tf : this.includeFilters) {
    if (tf.match(metadataReader,this.metadataReaderFactory)) {
      return !shouldSkip(metadataReader);
    }
  }
  return false;
}
