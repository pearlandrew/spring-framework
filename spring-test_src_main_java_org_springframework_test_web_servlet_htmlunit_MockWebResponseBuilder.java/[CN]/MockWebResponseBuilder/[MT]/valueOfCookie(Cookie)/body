{
  Date expires=null;
  if (cookie.getMaxAge() > -1) {
    expires=new Date(System.currentTimeMillis() + cookie.getMaxAge() * 1000);
  }
  return new com.gargoylesoftware.htmlunit.util.Cookie(cookie.getDomain(),cookie.getName(),cookie.getValue(),cookie.getPath(),expires,cookie.getSecure(),cookie.isHttpOnly()).toString();
}
