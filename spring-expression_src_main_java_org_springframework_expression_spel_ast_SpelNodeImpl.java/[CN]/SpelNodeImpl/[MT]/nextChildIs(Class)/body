{
  if (parent != null) {
    SpelNodeImpl[] peers=parent.children;
    for (int i=0, max=peers.length; i < max; i++) {
      if (peers[i] == this) {
        if ((i + 1) >= max) {
          return false;
        }
 else {
          Class clazz=peers[i + 1].getClass();
          for (          Class desiredClazz : clazzes) {
            if (clazz.equals(desiredClazz)) {
              return true;
            }
          }
          return false;
        }
      }
    }
  }
  return false;
}
