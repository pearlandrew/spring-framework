{
  for (  Map.Entry<String,List<Object>> entry : parts.entrySet()) {
    String name=entry.getKey();
    for (    Object part : entry.getValue()) {
      if (part != null) {
        writeBoundary(boundary,os);
        HttpEntity<?> entity=getEntity(part);
        writePart(name,entity,os);
        writeNewLine(os);
      }
    }
  }
}
