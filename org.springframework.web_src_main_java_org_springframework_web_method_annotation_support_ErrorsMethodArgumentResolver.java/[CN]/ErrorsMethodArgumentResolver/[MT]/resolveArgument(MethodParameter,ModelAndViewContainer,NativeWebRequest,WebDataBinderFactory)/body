{
  ModelMap model=mavContainer.getModel();
  if (model.size() > 0) {
    int lastIndex=model.size() - 1;
    String lastKey=new ArrayList<String>(model.keySet()).get(lastIndex);
    if (lastKey.startsWith(BindingResult.MODEL_KEY_PREFIX)) {
      return model.get(lastKey);
    }
  }
  throw new IllegalStateException("An Errors/BindingResult argument must follow a model attribute argument. " + "Check your handler method signature: " + parameter.getMethod());
}
