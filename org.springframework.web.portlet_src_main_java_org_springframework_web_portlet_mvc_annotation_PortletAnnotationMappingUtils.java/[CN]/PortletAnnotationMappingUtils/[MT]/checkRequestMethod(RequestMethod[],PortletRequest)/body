{
  if (methods.length == 0) {
    return true;
  }
  if (!(request instanceof ClientDataRequest)) {
    return false;
  }
  String method=((ClientDataRequest)request).getMethod();
  for (  RequestMethod candidate : methods) {
    if (method.equals(candidate.name())) {
      return true;
    }
  }
  return false;
}
