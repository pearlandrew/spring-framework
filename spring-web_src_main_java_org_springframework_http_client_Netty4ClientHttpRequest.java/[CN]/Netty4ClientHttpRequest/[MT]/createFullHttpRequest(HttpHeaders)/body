{
  io.netty.handler.codec.http.HttpMethod nettyMethod=io.netty.handler.codec.http.HttpMethod.valueOf(method.name());
  FullHttpRequest nettyRequest=new DefaultFullHttpRequest(HttpVersion.HTTP_1_1,nettyMethod,this.uri.getRawPath(),this.body.buffer());
  nettyRequest.headers().set(io.netty.handler.codec.http.HttpHeaders.Names.HOST,uri.getHost());
  nettyRequest.headers().set(io.netty.handler.codec.http.HttpHeaders.Names.CONNECTION,io.netty.handler.codec.http.HttpHeaders.Values.CLOSE);
  for (  Map.Entry<String,List<String>> entry : headers.entrySet()) {
    nettyRequest.headers().add(entry.getKey(),entry.getValue());
  }
  return nettyRequest;
}
