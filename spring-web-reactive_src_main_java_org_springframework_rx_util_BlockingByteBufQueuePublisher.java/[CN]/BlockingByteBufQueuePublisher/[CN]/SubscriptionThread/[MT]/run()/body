{
  try {
    while (!Thread.currentThread().isInterrupted())     if ((l < requestCount || requestCount == Long.MAX_VALUE) && queue.isHeadBuffer()) {
      subscriber.onNext(queue.pollBuffer());
      l++;
    }
 else     if (queue.isHeadError()) {
      subscriber.onError(queue.pollError());
      break;
    }
 else     if (queue.isComplete()) {
      subscriber.onComplete();
      break;
    }
  }
 catch (  InterruptedException ex) {
  }
}
