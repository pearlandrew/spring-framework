{
synchronized (this) {
    if (isSetOrExpired()) {
      return false;
    }
    this.result=result;
    if (this.resultHandler != null) {
      try {
        this.resultHandler.handleResult(this.result);
      }
 catch (      Throwable t) {
        logger.trace("DeferredResult not handled",t);
        return false;
      }
    }
  }
  return true;
}
