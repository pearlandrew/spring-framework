{
  StringBuilder sb=new StringBuilder();
  if (getChildCount() == 1) {
    sb.append("{|| ");
    sb.append(getChild(0).toStringAST());
    sb.append(" }");
  }
 else {
    sb.append("{|");
    sb.append(getChild(0).toStringAST());
    sb.append("| ");
    sb.append(getChild(1).toStringAST());
    sb.append(" }");
  }
  return sb.toString();
}
