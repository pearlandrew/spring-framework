{
  Class<?> testClass=ClassesFoo.class;
  Class<? extends ContextLoader> expectedContextLoaderClass=GenericPropertiesContextLoader.class;
  MergedContextConfiguration mergedConfig=ContextLoaderUtils.buildMergedContextConfiguration(testClass,expectedContextLoaderClass.getName());
  assertMergedContextConfiguration(mergedConfig,testClass,EMPTY_STRING_ARRAY,new Class<?>[]{FooConfig.class},expectedContextLoaderClass);
}
