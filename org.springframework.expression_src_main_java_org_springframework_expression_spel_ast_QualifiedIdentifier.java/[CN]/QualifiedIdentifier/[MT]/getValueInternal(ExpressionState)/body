{
  if (this.value == null) {
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < getChildCount(); i++) {
      if (i > 0) {
        sb.append(".");
      }
      sb.append(getChild(i).getValueInternal(state).getValue());
    }
    this.value=new TypedValue(sb.toString(),STRING_TYPE_DESCRIPTOR);
  }
  return this.value;
}
