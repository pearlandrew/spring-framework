{
  if (requiredType != null) {
    PortletResponse response=getResponse();
    while (response != null) {
      if (requiredType.isInstance(response)) {
        return (T)response;
      }
 else       if (response instanceof PortletResponseWrapper) {
        response=((PortletResponseWrapper)response).getResponse();
      }
 else {
        response=null;
      }
    }
  }
  return null;
}
