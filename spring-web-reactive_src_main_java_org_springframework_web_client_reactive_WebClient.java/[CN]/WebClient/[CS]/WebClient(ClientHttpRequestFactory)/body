{
  this.requestFactory=requestFactory;
  DataBufferAllocator allocator=new DefaultDataBufferAllocator();
  this.messageEncoders=Arrays.asList(new ByteBufferEncoder(),new StringEncoder(),new JacksonJsonEncoder());
  this.messageDecoders=Arrays.asList(new ByteBufferDecoder(),new StringDecoder(),new JacksonJsonDecoder(new JsonObjectDecoder()));
}
