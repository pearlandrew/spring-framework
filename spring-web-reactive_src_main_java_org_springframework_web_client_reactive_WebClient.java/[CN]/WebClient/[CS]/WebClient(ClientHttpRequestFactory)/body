{
  this.requestFactory=requestFactory;
  DataBufferAllocator allocator=new DefaultDataBufferAllocator();
  this.messageEncoders=Arrays.asList(new ByteBufferEncoder(allocator),new StringEncoder(allocator),new JacksonJsonEncoder(allocator));
  this.messageDecoders=Arrays.asList(new ByteBufferDecoder(),new StringDecoder(allocator),new JacksonJsonDecoder(new JsonObjectDecoder()));
}
