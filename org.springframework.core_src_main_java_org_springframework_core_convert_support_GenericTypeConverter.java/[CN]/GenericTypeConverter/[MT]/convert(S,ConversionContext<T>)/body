{
  if (source == null) {
    return null;
  }
  ConversionExecutor executor=getConversionExecutor(source.getClass(),context);
  if (executor != null) {
    return (T)executor.execute(source);
  }
 else {
    if (parent != null) {
      return parent.convert(source,context);
    }
 else {
      throw new ConverterNotFoundException(source.getClass(),context.getType(),"No converter found that can convert from sourceType [" + source.getClass().getName() + "] to targetType ["+ context.getName()+ "]");
    }
  }
}
