{
  final String internalName;
  if ("V".equals(typeDescriptor))   return Void.class.getName();
  if ("I".equals(typeDescriptor))   return Integer.class.getName();
  if ("Z".equals(typeDescriptor))   return Boolean.class.getName();
  if (typeDescriptor.startsWith("[["))   internalName=typeDescriptor.substring(3);
 else   if (typeDescriptor.startsWith("["))   internalName=typeDescriptor.substring(2);
 else   internalName=typeDescriptor.substring(1);
  String className=internalName.replace('/','.');
  if (className.endsWith(";"))   className=className.substring(0,internalName.length() - 1);
  return className;
}
