{
  this.includeFilters.add(new AnnotationTypeFilter(Component.class));
  this.includeFilters.add(new AnnotationTypeFilter(Scope.class));
  ClassLoader cl=ClassPathScanningCandidateComponentProvider.class.getClassLoader();
  try {
    this.includeFilters.add(new AnnotationTypeFilter(((Class<? extends Annotation>)cl.loadClass("javax.inject.Named"))));
    this.includeFilters.add(new AnnotationTypeFilter(((Class<? extends Annotation>)cl.loadClass("javax.inject.Scope"))));
  }
 catch (  ClassNotFoundException ex) {
  }
}
