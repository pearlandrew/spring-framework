{
  try {
    BinaryMessage message=new BinaryMessage(payload,offset,len);
    if (logger.isTraceEnabled()) {
      logger.trace("Received binary data for WebSocket session id=" + this.session.getId() + ": "+ message);
    }
    if (this.handler instanceof BinaryMessageHandler) {
      ((BinaryMessageHandler)this.handler).handleBinaryMessage(message,this.session);
    }
  }
 catch (  Throwable ex) {
    tryCloseWithError(ex);
  }
}
