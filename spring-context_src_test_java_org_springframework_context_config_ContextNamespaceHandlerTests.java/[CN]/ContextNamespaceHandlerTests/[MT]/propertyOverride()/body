{
  ApplicationContext applicationContext=new ClassPathXmlApplicationContext("contextNamespaceHandlerTests-override.xml",getClass());
  Map<String,PropertyOverrideConfigurer> beans=applicationContext.getBeansOfType(PropertyOverrideConfigurer.class);
  assertFalse("No PropertyOverrideConfigurer found",beans.isEmpty());
  Date date=(Date)applicationContext.getBean("date");
  Calendar calendar=Calendar.getInstance();
  calendar.setTime(date);
  assertEquals("No properties overriden",42,calendar.get(Calendar.MINUTE));
}
