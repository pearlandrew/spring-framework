{
  List list=Arrays.asList(new String[]{"0","1"});
  CompositeIterator it=new CompositeIterator();
  it.add(list.iterator());
  it.hasNext();
  try {
    it.add(list.iterator());
    fail();
  }
 catch (  IllegalStateException e) {
  }
  it=new CompositeIterator();
  it.add(list.iterator());
  it.next();
  try {
    it.add(list.iterator());
    fail();
  }
 catch (  IllegalStateException e) {
  }
}
