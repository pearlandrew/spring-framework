{
  Class sourceClass=sourceType.getObjectType();
  return Number.class.isAssignableFrom(sourceClass) || Boolean.class.equals(sourceClass) || Character.class.equals(sourceClass)|| CharSequence.class.isAssignableFrom(sourceClass)|| StringWriter.class.isAssignableFrom(sourceClass)|| sourceClass.isEnum()|| ObjectToObjectConverter.hasValueOfMethodOrConstructor(sourceClass,String.class);
}
