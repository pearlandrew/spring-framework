{
  if (AbstractBeanDefinition.INFER_METHOD.equals(beanDefinition.getDestroyMethodName()) || (beanDefinition.getDestroyMethodName() == null && closeableInterface.isInstance(bean))) {
    if (!(bean instanceof DisposableBean)) {
      try {
        return bean.getClass().getMethod(CLOSE_METHOD_NAME).getName();
      }
 catch (      NoSuchMethodException ex) {
      }
    }
    return null;
  }
  return beanDefinition.getDestroyMethodName();
}
