{
  DefaultListableBeanFactory beanFactory=new DefaultListableBeanFactory();
  beanFactory.registerBeanDefinition("unloadedConfig",rootBeanDefinition(UnloadedConfig.class.getName()).getBeanDefinition());
  beanFactory.registerBeanDefinition("loadedConfig",rootBeanDefinition(LoadedConfig.class).getBeanDefinition());
  new ConfigurationClassPostProcessor().postProcessBeanFactory(beanFactory);
  beanFactory.getBean("foo");
  beanFactory.getBean("bar");
}
