{
  WebApplicationContext wac=initWebAppContext();
  ServletContext servletContext=wac.getServletContext();
  MockServletConfig mockServletConfig=new MockServletConfig(servletContext);
  Filter[] filterArray=this.filters.toArray(new Filter[this.filters.size()]);
  return super.createMockMvc(filterArray,mockServletConfig,wac,this.defaultRequestBuilder,this.globalResultMatchers,this.globalResultHandlers,this.dispatchOptions);
}
