{
  Method withRulesMethod=ReflectionUtils.findMethod(getClass(),"withRules",FrameworkMethod.class,Object.class,Statement.class);
  if (withRulesMethod != null) {
    ReflectionUtils.makeAccessible(withRulesMethod);
    statement=(Statement)ReflectionUtils.invokeMethod(withRulesMethod,this,frameworkMethod,testInstance,statement);
  }
  return statement;
}
