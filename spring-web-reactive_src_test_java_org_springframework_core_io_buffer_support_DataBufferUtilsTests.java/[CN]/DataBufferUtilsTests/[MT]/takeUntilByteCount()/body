{
  Flux<DataBuffer> flux=Flux.just(stringBuffer("foo"),stringBuffer("bar"),stringBuffer("baz"));
  Flux<DataBuffer> result=DataBufferUtils.takeUntilByteCount(flux,5L);
  TestSubscriber<DataBuffer> testSubscriber=new TestSubscriber<>();
  testSubscriber.bindTo(result).assertNoError().assertComplete().assertValues(stringBuffer("foo"),stringBuffer("ba"));
}
