{
  return new ToStringCreator(this).append("dataSource",dataSource).append("transactionManager",transactionManager).append("transactionMode",transactionMode).append("encoding",encoding).append("separator",separator).append("commentPrefix",commentPrefix).append("blockCommentStartDelimiter",blockCommentStartDelimiter).append("blockCommentEndDelimiter",blockCommentEndDelimiter).append("errorMode",errorMode).toString();
}
