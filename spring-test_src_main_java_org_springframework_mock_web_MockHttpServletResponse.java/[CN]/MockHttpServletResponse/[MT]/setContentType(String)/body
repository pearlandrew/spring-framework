{
  this.contentType=contentType;
  if (contentType != null) {
    int charsetIndex=contentType.toLowerCase().indexOf(CHARSET_PREFIX);
    if (charsetIndex != -1) {
      this.characterEncoding=contentType.substring(charsetIndex + CHARSET_PREFIX.length());
      this.charset=true;
    }
    updateContentTypeHeader();
  }
}
