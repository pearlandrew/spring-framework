{
  if (descriptor.length() != 1) {
    if (descriptor.charAt(0) == '[') {
      if (isPrimitiveArray(descriptor)) {
        mv.visitTypeInsn(CHECKCAST,descriptor);
      }
 else {
        mv.visitTypeInsn(CHECKCAST,descriptor + ";");
      }
    }
 else {
      mv.visitTypeInsn(CHECKCAST,descriptor.substring(1));
    }
  }
}
