{
  if (descriptor == null) {
    return false;
  }
  if (descriptor.length() == 1) {
    return ("DFJZI".indexOf(descriptor.charAt(0)) != -1);
  }
  if (descriptor.startsWith("Ljava/lang/")) {
    if (descriptor.equals("Ljava/lang/Double") || descriptor.equals("Ljava/lang/Integer") || descriptor.equals("Ljava/lang/Float")|| descriptor.equals("Ljava/lang/Long")|| descriptor.equals("Ljava/lang/Boolean")) {
      return true;
    }
  }
  return false;
}
