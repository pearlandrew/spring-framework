{
  if (this.headers == null) {
    this.headers=new HttpHeaders();
    int i=1;
    while (true) {
      String name=this.connection.getHeaderFieldKey(i);
      if (!StringUtils.hasLength(name)) {
        break;
      }
      this.headers.add(name,this.connection.getHeaderField(i));
      i++;
    }
  }
  return this.headers;
}
