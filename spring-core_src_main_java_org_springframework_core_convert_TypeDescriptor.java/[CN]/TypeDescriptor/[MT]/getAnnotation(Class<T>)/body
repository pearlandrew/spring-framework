{
  for (  Annotation annotation : getAnnotations()) {
    if (annotation.annotationType().equals(annotationType)) {
      return (T)annotation;
    }
  }
  for (  Annotation metaAnn : getAnnotations()) {
    T ann=metaAnn.annotationType().getAnnotation(annotationType);
    if (ann != null) {
      return ann;
    }
  }
  return null;
}
