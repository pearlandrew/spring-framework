{
  Object value=null;
  if (this.cached && this.shareable) {
    value=this.cachedFieldValue;
  }
synchronized (this) {
    if (!this.cached) {
      value=getResource(this,requestingBeanName);
      if (value != null && this.shareable) {
        this.cachedFieldValue=value;
        this.cached=true;
      }
    }
  }
  return value;
}
