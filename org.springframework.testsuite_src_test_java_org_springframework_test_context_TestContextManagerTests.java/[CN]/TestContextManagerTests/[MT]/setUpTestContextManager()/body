{
  final Method testMethod=ExampleTestCase.class.getDeclaredMethod("exampleTestMethod",(Class<?>[])null);
  this.testContextManager=new TestContextManager(ExampleTestCase.class);
  this.testContextManager.registerTestExecutionListeners(new NamedTestExecutionListener(FIRST),new NamedTestExecutionListener(SECOND),new NamedTestExecutionListener(THIRD));
  assertEquals("Verifying the number of registered TestExecutionListeners.",6,this.testContextManager.getTestExecutionListeners().size());
  this.testContextManager.beforeTestMethod(new ExampleTestCase(),testMethod);
}
