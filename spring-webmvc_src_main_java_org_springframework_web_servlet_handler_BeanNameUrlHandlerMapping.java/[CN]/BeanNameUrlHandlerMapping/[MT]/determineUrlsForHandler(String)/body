{
  List<String> urls=new ArrayList<String>();
  if (beanName.startsWith("/")) {
    urls.add(beanName);
  }
  String[] aliases=getApplicationContext().getAliases(beanName);
  for (  String alias : aliases) {
    if (alias.startsWith("/")) {
      urls.add(alias);
    }
  }
  return StringUtils.toStringArray(urls);
}
