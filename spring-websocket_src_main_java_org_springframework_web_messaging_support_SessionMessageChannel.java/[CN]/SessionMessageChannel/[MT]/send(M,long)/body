{
  PubSubHeaderAccesssor headers=PubSubHeaderAccesssor.wrap(message);
  headers.setSessionId(this.sessionId);
  Object payload=message.getPayload();
  @SuppressWarnings("unchecked") M messageToSend=(M)MessageBuilder.withPayload(payload).copyHeaders(headers.toHeaders()).build();
  this.delegate.send(messageToSend);
  return true;
}
