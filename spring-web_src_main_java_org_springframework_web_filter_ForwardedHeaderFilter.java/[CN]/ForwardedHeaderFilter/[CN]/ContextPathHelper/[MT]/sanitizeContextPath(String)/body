{
  contextPath=contextPath.trim();
  if (contextPath.isEmpty()) {
    return contextPath;
  }
  if (contextPath.equals("/")) {
    return "/";
  }
  if (contextPath.charAt(0) != '/') {
    contextPath="/" + contextPath;
  }
  while (contextPath.endsWith("/")) {
    contextPath=contextPath.substring(0,contextPath.length() - 1);
  }
  return contextPath;
}
