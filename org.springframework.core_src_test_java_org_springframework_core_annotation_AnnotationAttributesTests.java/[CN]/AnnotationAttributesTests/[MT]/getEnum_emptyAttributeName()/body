{
  AnnotationAttributes a=new AnnotationAttributes();
  a.put("color","RED");
  try {
    a.getEnum("");
    fail();
  }
 catch (  IllegalArgumentException ex) {
    assertThat(ex.getMessage(),equalTo("attributeName must not be null or empty"));
  }
  try {
    a.getEnum(null);
    fail();
  }
 catch (  IllegalArgumentException ex) {
    assertThat(ex.getMessage(),equalTo("attributeName must not be null or empty"));
  }
}
