{
  assertNull(config.checkHeaders(null));
  assertNull(config.checkHeaders(Arrays.asList("header1")));
  config.addAllowedHeader("header2");
  assertNull(config.checkHeaders(Arrays.asList("header1")));
  config.setAllowedHeaders(new ArrayList<>());
  assertNull(config.checkHeaders(Arrays.asList("header1")));
}
