{
  MvcResult mvcResult=this.mockMvc.perform(get("/1").param("deferredResult","true")).andDo(print()).andExpect(request().asyncStarted()).andReturn();
  this.asyncController.onMessage("Joe");
  this.mockMvc.perform(asyncDispatch(mvcResult)).andDo(print()).andExpect(status().isOk()).andExpect(content().contentType(MediaType.APPLICATION_JSON)).andExpect(content().string("{\"name\":\"Joe\",\"someDouble\":0.0,\"someBoolean\":false}"));
}
