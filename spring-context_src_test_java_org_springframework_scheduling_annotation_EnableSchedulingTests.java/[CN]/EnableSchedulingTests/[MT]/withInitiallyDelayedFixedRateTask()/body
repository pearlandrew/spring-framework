{
  AnnotationConfigApplicationContext ctx=new AnnotationConfigApplicationContext(FixedRateTaskConfig_withInitialDelay.class);
  Thread.sleep(1950);
  AtomicInteger counter=ctx.getBean(AtomicInteger.class);
  ctx.close();
  assertThat(counter.get(),both(greaterThan(0)).and(lessThanOrEqualTo(10)));
}
