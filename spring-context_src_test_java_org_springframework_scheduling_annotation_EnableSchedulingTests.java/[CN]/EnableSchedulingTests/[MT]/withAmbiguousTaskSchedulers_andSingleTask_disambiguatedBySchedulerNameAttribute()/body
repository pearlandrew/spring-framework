{
  Assume.group(TestGroup.PERFORMANCE);
  AnnotationConfigApplicationContext ctx=new AnnotationConfigApplicationContext(SchedulingEnabled_withAmbiguousTaskSchedulers_andSingleTask_disambiguatedBySchedulerNameAttribute.class);
  Thread.sleep(20);
  assertThat(ctx.getBean(ThreadAwareWorker.class).executedByThread,startsWith("explicitScheduler2-"));
  ctx.close();
}
