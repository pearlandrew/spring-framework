{
  Charset iso88591=Charset.forName("ISO-8859-1");
  String payload="H\u00e9llo W\u00f6rld";
  Message<byte[]> message=MessageBuilder.withPayload(payload.getBytes(iso88591)).setHeader(MessageHeaders.CONTENT_TYPE,new MimeType("text","plain",iso88591)).build();
  assertEquals(payload,this.converter.fromMessage(message,String.class));
}
