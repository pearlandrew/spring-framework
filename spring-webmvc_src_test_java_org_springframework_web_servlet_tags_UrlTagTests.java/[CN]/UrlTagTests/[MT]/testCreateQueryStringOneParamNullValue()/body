{
  List<Param> params=new LinkedList<Param>();
  Set<String> usedParams=new HashSet<String>();
  Param param=new Param();
  param.setName("name");
  param.setValue(null);
  params.add(param);
  String queryString=tag.createQueryString(params,usedParams,true);
  assertEquals("?name",queryString);
}
