{
  if (this.elementType != null) {
    return this.elementType;
  }
 else {
    if (isArray()) {
      return TypeDescriptor.valueOf(getArrayComponentType());
    }
 else     if (isCollection()) {
      return TypeDescriptor.valueOf(getCollectionElementType());
    }
 else {
      return TypeDescriptor.NULL;
    }
  }
}
