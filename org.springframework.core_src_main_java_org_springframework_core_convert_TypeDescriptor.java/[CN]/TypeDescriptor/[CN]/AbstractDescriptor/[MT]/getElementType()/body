{
  if (isCollection()) {
    Class<?> elementType=wildcard(getCollectionElementClass());
    return new TypeDescriptor(nested(elementType,0));
  }
 else   if (isArray()) {
    Class<?> elementType=getType().getComponentType();
    return new TypeDescriptor(nested(elementType,0));
  }
 else {
    return TypeDescriptor.NULL;
  }
}
