{
  ObjectSelector<String> selector=new ObjectSelector<String>(key);
  final Registration<Consumer<Event<Object>>> registration=this.reactor.on(selector,new Consumer<Event<Object>>(){
    @Override public void accept(    Event<Object> event){
      consumer.accept(event.getData());
    }
  }
);
  return new EventRegistration(){
    @Override public String getRegistrationKey(){
      return key;
    }
    @Override public void cancel(){
      registration.cancel();
    }
  }
;
}
