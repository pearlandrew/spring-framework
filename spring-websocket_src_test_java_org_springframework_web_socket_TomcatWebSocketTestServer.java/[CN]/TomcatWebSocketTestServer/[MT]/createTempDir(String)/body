{
  try {
    File tempFolder=File.createTempFile(prefix + ".","." + getPort());
    tempFolder.delete();
    tempFolder.mkdir();
    tempFolder.deleteOnExit();
    return tempFolder;
  }
 catch (  IOException ex) {
    throw new NestedRuntimeException("Unable to create temp directory",ex){
    }
;
  }
}
